{
  if (this.refreshExecutors != null) {
    this.refreshExecutors.shutdown();
  }
  this.refreshExecutors=MoreExecutors.listeningDecorator(Executors.newScheduledThreadPool(graphFig.getShardCacheRefreshWorkerCount()));
  this.graphs=CacheBuilder.newBuilder().refreshAfterWrite(graphFig.getShardCacheTimeout(),TimeUnit.MILLISECONDS).maximumWeight(MAX_WEIGHT_PER_ELEMENT * graphFig.getShardCacheSize()).weigher(new ShardWeigher()).build(new ShardCacheLoader());
}
