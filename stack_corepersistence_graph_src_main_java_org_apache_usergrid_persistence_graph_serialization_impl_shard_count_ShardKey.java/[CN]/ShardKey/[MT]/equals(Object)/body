{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  final ShardKey shardKey=(ShardKey)o;
  if (!directedEdgeMeta.equals(shardKey.directedEdgeMeta)) {
    return false;
  }
  if (!scope.equals(shardKey.scope)) {
    return false;
  }
  if (!shard.equals(shardKey.shard)) {
    return false;
  }
  return true;
}
