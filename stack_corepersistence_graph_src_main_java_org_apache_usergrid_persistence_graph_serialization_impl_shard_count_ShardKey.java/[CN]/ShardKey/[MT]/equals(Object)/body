{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  final ShardKey shardKey=(ShardKey)o;
  if (shardId != shardKey.shardId) {
    return false;
  }
  if (!Arrays.equals(edgeTypes,shardKey.edgeTypes)) {
    return false;
  }
  if (!nodeId.equals(shardKey.nodeId)) {
    return false;
  }
  if (!scope.equals(shardKey.scope)) {
    return false;
  }
  return true;
}
