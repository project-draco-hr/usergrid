{
  OAuthResponse oauthResponse=OAuthResponse.errorResponse(HttpServletResponse.SC_UNAUTHORIZED).setRealm(REALM).setError(OAuthError.ResourceResponse.INVALID_TOKEN).buildHeaderMessage();
  throw new WebApplicationException(Response.status(Response.Status.UNAUTHORIZED).header(OAuth.HeaderType.WWW_AUTHENTICATE,oauthResponse.getHeader(OAuth.HeaderType.WWW_AUTHENTICATE)).build());
}
