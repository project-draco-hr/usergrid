{
  if (start != null) {
    C startEdge=createColumn(start);
    rangeBuilder.setStart(startEdge,getSerializer());
  }
 else {
    setTimeScan(rangeBuilder);
  }
  if (end != null) {
    C endEdge=createColumn(end);
    rangeBuilder.setEnd(endEdge,getSerializer());
  }
  setRangeOptions(rangeBuilder);
}
