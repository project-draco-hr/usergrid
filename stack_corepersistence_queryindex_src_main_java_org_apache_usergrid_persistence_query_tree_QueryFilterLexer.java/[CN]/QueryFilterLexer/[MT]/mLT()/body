{
  try {
    int _type=LT;
    int _channel=DEFAULT_TOKEN_CHANNEL;
    int alt1=2;
    int LA1_0=input.LA(1);
    if ((LA1_0 == '<')) {
      alt1=1;
    }
 else     if ((LA1_0 == 'l')) {
      alt1=2;
    }
 else {
      NoViableAltException nvae=new NoViableAltException("",1,0,input);
      throw nvae;
    }
switch (alt1) {
case 1:
{
        match('<');
      }
    break;
case 2:
{
    match("lt");
  }
break;
}
state.type=_type;
state.channel=_channel;
}
  finally {
}
}
