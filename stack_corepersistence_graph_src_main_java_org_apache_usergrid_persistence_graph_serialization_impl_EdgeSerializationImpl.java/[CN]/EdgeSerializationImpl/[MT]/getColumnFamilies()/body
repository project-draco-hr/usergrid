{
  return Arrays.asList(graphCf(sourceNodeEdgesCf),graphCf(targetNodeEdgesCf),graphCf(sourceNodeTargetTypeCf),graphCf(targetNodeSourceTypeCf),new MultiTennantColumnFamilyDefinition(graphEdgeVersionsCf,BytesType.class.getSimpleName(),ColumnTypes.LONG_TYPE_REVERSED,BytesType.class.getSimpleName(),MultiTennantColumnFamilyDefinition.CacheOption.KEYS));
}
