{
  final RangeBuilder rangeBuilder=new RangeBuilder().setLimit(cassandraConfig.getScanPageSize());
  searcher.setRange(rangeBuilder);
  final ScopedRowKey<OrganizationScope,R> rowKey=searcher.next();
  RowQuery<ScopedRowKey<OrganizationScope,R>,C> query=keyspace.prepareQuery(cf).setConsistencyLevel(cassandraConfig.getReadCL()).getKey(rowKey).autoPaginate(true).withColumnRange(rangeBuilder.build());
  currentColumnIterator=new ColumnNameIterator<C,T>(query,searcher,searcher.hasPage());
}
