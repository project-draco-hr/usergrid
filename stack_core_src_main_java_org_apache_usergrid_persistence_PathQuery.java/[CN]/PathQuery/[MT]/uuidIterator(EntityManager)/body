{
  if (head != null) {
    return new PagingResultsIterator(getHeadResults(em),Level.IDS);
  }
 else {
    Query q=query;
    if (query.getResultsLevel() != Level.IDS) {
      q=new Query(q);
      q.setResultsLevel(Level.IDS);
    }
    return new MultiQueryIterator(em,source.uuidIterator(em),q);
  }
}
