{
  CollectionInfo collection=getCollection(containerType,collectionName);
  if (collection == null) {
    return false;
  }
  EntityInfo item=getEntityInfo(collection.getType());
  if (item == null) {
    return false;
  }
  PropertyInfo property=item.getAliasPropertyObject();
  if (property == null) {
    return false;
  }
  return property.isPathBasedName();
}
