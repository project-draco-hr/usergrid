{
  if (o instanceof String) {
    String s=(String)o;
    if (UUID_PATTERN.matcher(s).matches()) {
      try {
        return UUID.fromString(s);
      }
 catch (      IllegalArgumentException e) {
        LOG.warn("Argument to UUID.fromString({}) was invalid.",s,e);
      }
    }
  }
  return null;
}
