{
  String orgName=clientSetup.getOrganizationName().toLowerCase();
  String appName=clientSetup.getAppName().toLowerCase();
  try {
    String response=target().path(String.format("/%s/%s/apm/apigeeMobileConfig",orgName,appName)).request().accept(MediaType.APPLICATION_JSON).get(String.class);
    JsonNode node=mapper.readTree(response);
    assertTrue("it's valid json for APM",node.has("instaOpsApplicationId"));
  }
 catch (  ClientErrorException uie) {
    assertNotEquals("APM Config API exists",Response.Status.NOT_FOUND,uie.getResponse().getStatus());
  }
}
