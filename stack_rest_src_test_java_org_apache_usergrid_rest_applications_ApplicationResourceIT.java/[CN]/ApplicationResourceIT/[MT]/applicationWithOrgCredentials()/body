{
  String clientId=setup.getMgmtSvc().getClientIdForOrganization(orgInfo.getUuid());
  String clientSecret=setup.getMgmtSvc().getClientSecretForOrganization(orgInfo.getUuid());
  refreshIndex(orgInfo.getName(),appInfo.getName());
  JsonNode node=mapper.readTree(resource().path(orgAppPath + "users").queryParam("client_id",clientId).queryParam("client_secret",clientSecret).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).get(String.class));
  assertNotNull(node.get("entities"));
}
