{
  Credentials orgCredentials=getOrgCredentials();
  ApiResponse apiResponse=this.org().app(clientSetup.getAppName()).getResource(false).queryParam("grant_type","client_credentials").queryParam("client_id",orgCredentials.getClientId()).queryParam("client_secret",orgCredentials.getClientSecret()).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).get(ApiResponse.class);
  assertNotNull(apiResponse);
  assertNull(apiResponse.getError());
}
