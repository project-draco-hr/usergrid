{
  String apiResponse=clientSetup.getRestClient().management().authorize().getResource(false).queryParam("response_type","code").queryParam("client_id","invalid_client_id").queryParam("redirect_uri","http://www.my_test.com").accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).get(String.class);
  assertTrue(apiResponse.contains("Unable to authenticate (OAuth). Invalid client_id."));
}
