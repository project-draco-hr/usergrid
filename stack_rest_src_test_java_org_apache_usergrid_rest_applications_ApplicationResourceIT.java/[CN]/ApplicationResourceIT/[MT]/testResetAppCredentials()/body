{
  String mgmtToken=adminToken();
  refreshIndex(orgInfo.getName(),appInfo.getName());
  JsonNode node=mapper.readTree(resource().path(orgAppPath + "credentials").queryParam("access_token",mgmtToken).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(String.class));
  assertEquals("ok",node.get("status").textValue());
  logNode(node);
}
