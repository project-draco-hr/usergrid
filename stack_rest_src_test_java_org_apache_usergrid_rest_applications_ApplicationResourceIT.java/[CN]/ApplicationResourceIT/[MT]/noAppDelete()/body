{
  String mgmtToken=adminToken();
  Status status=null;
  JsonNode node=null;
  try {
    node=mapper.readTree(resource().path("/test-organization/test-app").queryParam("access_token",mgmtToken).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).delete(String.class));
  }
 catch (  UniformInterfaceException uie) {
    status=uie.getResponse().getClientResponseStatus();
  }
  assertEquals(Status.NOT_IMPLEMENTED,status);
}
