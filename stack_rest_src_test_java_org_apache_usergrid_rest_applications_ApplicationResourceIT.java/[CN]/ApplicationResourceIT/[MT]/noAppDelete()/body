{
  String orgName=clientSetup.getOrganizationName().toLowerCase();
  String appName=clientSetup.getAppName().toLowerCase();
  ApiResponse apiResponse=target().path(String.format("/%s/%s",orgName,appName)).queryParam("access_token",this.getAdminToken().getAccessToken()).request().accept(MediaType.APPLICATION_JSON).delete(ApiResponse.class);
  assertNotNull(apiResponse.getError());
}
