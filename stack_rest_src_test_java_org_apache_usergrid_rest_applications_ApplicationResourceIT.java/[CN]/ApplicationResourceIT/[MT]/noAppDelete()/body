{
  String orgName=clientSetup.getOrganizationName().toLowerCase();
  String appName=clientSetup.getAppName().toLowerCase();
  ApiResponse apiResponse=resource().path(String.format("/%s/%s",orgName,appName)).queryParam("access_token",this.getAdminToken().getAccessToken()).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).delete(ApiResponse.class);
  assertNotNull(apiResponse.getError());
}
