{
  Map<B,Set<C>> map_b=map.get(a);
  if (map_b == null) {
    if (ignore_case && (b instanceof String)) {
      map_b=(Map<B,Set<C>>)new TreeMap<String,Set<C>>(String.CASE_INSENSITIVE_ORDER);
    }
 else {
      map_b=new LinkedHashMap<B,Set<C>>();
    }
    map.put(a,map_b);
  }
  addMapSet(map_b,ignore_case,b,c);
}
