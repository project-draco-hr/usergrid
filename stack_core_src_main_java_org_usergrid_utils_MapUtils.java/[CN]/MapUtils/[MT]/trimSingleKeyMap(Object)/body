{
  if (obj == null) {
    return null;
  }
  if (!(obj instanceof Map)) {
    return obj;
  }
  @SuppressWarnings("unchecked") Map<Object,Object> map=(Map<Object,Object>)obj;
  if (map.size() == 1) {
    Iterator<Map.Entry<Object,Object>> i=map.entrySet().iterator();
    return i.next().getValue();
  }
  return map;
}
