{
  final long timestamp=version.timestamp();
  final ScopedRowKey<OrganizationScope,Id> targetKey=new ScopedRowKey<OrganizationScope,Id>(scope,rowKeyId);
  final MutationBatch batch=keyspace.prepareMutationBatch();
  batch.withRow(cf,targetKey).setTimestamp(timestamp).deleteColumn(edgeType);
  return batch;
}
