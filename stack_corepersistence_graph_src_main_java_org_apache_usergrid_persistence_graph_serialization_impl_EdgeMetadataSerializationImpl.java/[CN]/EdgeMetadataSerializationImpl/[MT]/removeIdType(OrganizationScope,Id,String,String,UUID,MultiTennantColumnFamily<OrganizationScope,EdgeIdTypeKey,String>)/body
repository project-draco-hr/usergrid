{
  MutationBatch batch=keyspace.prepareMutationBatch();
  final long timestamp=CassUtils.getTimestamp(version);
  final ScopedRowKey<OrganizationScope,EdgeIdTypeKey> rowKey=new ScopedRowKey<OrganizationScope,EdgeIdTypeKey>(scope,new EdgeIdTypeKey(rowId,edgeType));
  batch.withRow(cf,rowKey).setTimestamp(timestamp).deleteColumn(idType);
  return batch;
}
