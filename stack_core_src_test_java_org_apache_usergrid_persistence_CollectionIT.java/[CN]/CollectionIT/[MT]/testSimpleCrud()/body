{
  LOG.debug("testSimpleCrud");
  app.put("username","edanuff");
  app.put("email","ed@anuff.com");
  Entity user=app.create("user");
  assertNotNull(user);
  user=app.get(user.getUuid(),"user");
  assertNotNull(user);
  app.remove(user);
  user=app.get(user.getUuid(),"user");
  assertNull(user);
}
