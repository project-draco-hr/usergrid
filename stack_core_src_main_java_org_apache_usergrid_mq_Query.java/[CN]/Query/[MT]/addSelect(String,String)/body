{
  if (select == null) {
    return this;
  }
  select=select.trim();
  if (select.equals("*")) {
    return this;
  }
  if (StringUtils.isNotEmpty(output)) {
    mergeSelectResults=true;
  }
 else {
    mergeSelectResults=false;
  }
  if (output == null) {
    output="";
  }
  selectSubjects.put(select,output);
  return this;
}
