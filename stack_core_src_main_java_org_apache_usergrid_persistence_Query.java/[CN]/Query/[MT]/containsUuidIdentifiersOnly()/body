{
  if (hasQueryPredicates()) {
    return false;
  }
  if ((identifiers == null) || identifiers.isEmpty()) {
    return false;
  }
  for (  Identifier identifier : identifiers) {
    if (identifier == null) {
      return true;
    }
    if (!identifier.isUUID()) {
      return false;
    }
  }
  return true;
}
