{
  TermQueryBuilder tqb=QueryBuilders.termQuery(STRING_PREFIX + ENTITYID_FIELDNAME,entityId.getUuid().toString().toLowerCase());
  DeleteByQueryResponse response=client.prepareDeleteByQuery(indexName).setQuery(tqb).execute().actionGet();
  logger.debug("Deleted entity {}:{} from all index scopes with response status = {}",new Object[]{entityId.getType(),entityId.getUuid(),response.status().toString()});
  maybeFlush();
  return this;
}
