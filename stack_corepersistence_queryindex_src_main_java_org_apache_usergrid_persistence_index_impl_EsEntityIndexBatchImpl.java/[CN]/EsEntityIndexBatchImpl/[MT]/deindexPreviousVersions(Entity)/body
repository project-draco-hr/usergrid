{
  FilteredQueryBuilder fqb=QueryBuilders.filteredQuery(QueryBuilders.termQuery(STRING_PREFIX + ENTITYID_FIELDNAME,entity.getId().getUuid().toString().toLowerCase()),FilterBuilders.rangeFilter(ENTITYVERSION_FIELDNAME).lt(entity.getVersion().timestamp()));
  DeleteByQueryResponse response=client.prepareDeleteByQuery(indexName).setQuery(fqb).execute().actionGet();
  logger.debug("Deleted entity {}:{} from all index scopes with response status = {}",new Object[]{entity.getId().getType(),entity.getId().getUuid(),response.status().toString()});
  maybeFlush();
  return this;
}
