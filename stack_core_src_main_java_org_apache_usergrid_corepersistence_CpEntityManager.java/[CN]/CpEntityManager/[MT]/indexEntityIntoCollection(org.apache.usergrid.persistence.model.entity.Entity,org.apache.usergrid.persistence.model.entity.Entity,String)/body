{
  final EntityIndex ei=getManagerCache().getEntityIndex(getApplicationScope());
  final EntityIndexBatch batch=ei.createBatch();
  IndexScope collectionIndexScope=new IndexScopeImpl(collectionEntity.getId(),CpNamingUtils.getCollectionScopeNameFromCollectionName(collName));
  batch.index(collectionIndexScope,memberEntity);
  Timer.Context timeIndexEntityCollection=corePersistanceTimer.time();
  batch.execute();
  timeIndexEntityCollection.stop();
}
