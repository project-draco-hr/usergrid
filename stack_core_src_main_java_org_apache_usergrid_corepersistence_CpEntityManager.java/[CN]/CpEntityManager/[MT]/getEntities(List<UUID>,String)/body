{
  ArrayList<Entity> entities=new ArrayList<Entity>();
  for (  UUID uuid : ids) {
    EntityRef ref=new SimpleEntityRef(type,uuid);
    Entity entity=null;
    try {
      entity=get(ref);
    }
 catch (    Exception ex) {
      logger.warn("Entity {}/{} not found",uuid,type);
    }
    if (entity != null) {
      entities.add(entity);
    }
  }
  return Results.fromEntities(entities);
}
