{
  CollectionScope collectionScope=new CollectionScopeImpl(applicationScope.getApplication(),applicationScope.getApplication(),CpNamingUtils.getCollectionScopeNameFromEntityType(collectionName));
  final EntityCollectionManager ecm=managerCache.getEntityCollectionManager(collectionScope);
  final Id results=ecm.getIdField(new StringField(propertyName,propertyValue.toString())).toBlocking().lastOrDefault(null);
  return results;
}
