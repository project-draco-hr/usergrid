{
  org.apache.usergrid.persistence.model.entity.Entity cpEntity=new org.apache.usergrid.persistence.model.entity.Entity(new SimpleId(entity.getUuid(),entity.getType()));
  cpEntity=CpEntityMapUtils.fromMap(cpEntity,entity.getProperties(),entity.getType(),true);
  cpEntity=CpEntityMapUtils.fromMap(cpEntity,entity.getDynamicProperties(),entity.getType(),true);
  return cpEntity;
}
