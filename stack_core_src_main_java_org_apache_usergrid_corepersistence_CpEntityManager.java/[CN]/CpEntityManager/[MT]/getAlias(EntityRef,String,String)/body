{
  Assert.notNull(ownerRef,"ownerRef is required");
  Assert.notNull(collectionType,"collectionType is required");
  Assert.notNull(aliasValue,"aliasValue is required");
  Map<String,EntityRef> results=getAlias(ownerRef,collectionType,Collections.singletonList(aliasValue));
  if (results == null || results.size() == 0) {
    return null;
  }
  if (results.size() > 1) {
    logger.warn("More than 1 entity with Owner id '{}' of type '{}' and alias '{}' exists. " + " This is a duplicate alias, and needs audited",new Object[]{ownerRef,collectionType,aliasValue});
  }
  return results.get(aliasValue);
}
