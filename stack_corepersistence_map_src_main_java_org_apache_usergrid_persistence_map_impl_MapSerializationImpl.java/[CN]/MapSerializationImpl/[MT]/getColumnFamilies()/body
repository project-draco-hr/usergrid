{
  final MultiTennantColumnFamilyDefinition mapEntries=new MultiTennantColumnFamilyDefinition(MAP_ENTRIES,BytesType.class.getSimpleName(),BytesType.class.getSimpleName(),BytesType.class.getSimpleName(),MultiTennantColumnFamilyDefinition.CacheOption.KEYS);
