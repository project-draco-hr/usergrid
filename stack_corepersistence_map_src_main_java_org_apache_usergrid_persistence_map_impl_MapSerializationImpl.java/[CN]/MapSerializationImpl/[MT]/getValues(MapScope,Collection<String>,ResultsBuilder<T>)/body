{
  final List<ScopedRowKey<MapEntryKey>> rowKeys=new ArrayList<>(keys.size());
  for (  final String key : keys) {
    final ScopedRowKey<MapEntryKey> entryRowKey=MapEntryKey.fromKey(scope,key);
    rowKeys.add(entryRowKey);
  }
  try {
    final Rows<ScopedRowKey<MapEntryKey>,Boolean> rows=keyspace.prepareQuery(MAP_ENTRIES).setConsistencyLevel(cassandraConfig.getReadCL()).getKeySlice(rowKeys).withColumnSlice(true).execute().getResult();
    return builder.buildResults(rows);
  }
 catch (  NotFoundException nfe) {
    return null;
  }
catch (  ConnectionException e) {
    throw new RuntimeException("Unable to connect to cassandra",e);
  }
}
