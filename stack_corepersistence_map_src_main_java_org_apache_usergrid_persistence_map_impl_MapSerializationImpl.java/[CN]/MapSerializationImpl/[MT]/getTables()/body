{
  final TableDefinition mapEntries=new TableDefinition(MAP_ENTRIES_TABLE,MAP_ENTRIES_PARTITION_KEYS,MAP_ENTRIES_COLUMN_KEYS,MAP_ENTRIES_COLUMNS,TableDefinition.CacheOption.KEYS,MAP_ENTRIES_CLUSTERING_ORDER);
  final TableDefinition mapKeys=new TableDefinition(MAP_KEYS_TABLE,MAP_KEYS_PARTITION_KEYS,MAP_KEYS_COLUMN_KEYS,MAP_KEYS_COLUMNS,TableDefinition.CacheOption.KEYS,MAP_KEYS_CLUSTERING_ORDER);
  return Arrays.asList(mapEntries,mapKeys);
}
