{
  Object value=em.getProperty(device,notifier.getName() + NotificationsService.NOTIFIER_ID_POSTFIX);
  if (value != null) {
    em.setProperty(device,notifier.getName() + NotificationsService.NOTIFIER_ID_POSTFIX,newProviderId);
  }
 else {
    value=em.getProperty(device,notifier.getUuid() + NotificationsService.NOTIFIER_ID_POSTFIX);
    if (value != null) {
      em.setProperty(device,notifier.getUuid() + NotificationsService.NOTIFIER_ID_POSTFIX,newProviderId);
    }
  }
}
