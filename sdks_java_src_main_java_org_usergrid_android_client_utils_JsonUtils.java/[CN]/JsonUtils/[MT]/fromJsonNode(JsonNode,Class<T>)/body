{
  try {
    return mapper.readValue(json,c);
  }
 catch (  JsonParseException e) {
    Log.e(TAG,"ERROR: " + e.getLocalizedMessage());
  }
catch (  JsonMappingException e) {
    Log.e(TAG,"ERROR: " + e.getLocalizedMessage());
  }
catch (  IOException e) {
    Log.e(TAG,"ERROR: " + e.getLocalizedMessage());
  }
  return null;
}
