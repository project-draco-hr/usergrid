{
  startSpring();
  setVerbose(line);
  openImportDirectory(line);
  importOrganizations();
  importApplications();
  importCollections();
  logger.info("Sleeping 35 seconds for batcher");
  Thread.sleep(35000);
  EntityManager em=emf.getEntityManager(MANAGEMENT_APPLICATION_ID);
  em.incrementApplicationCounter("import.counter.batcher",1);
}
