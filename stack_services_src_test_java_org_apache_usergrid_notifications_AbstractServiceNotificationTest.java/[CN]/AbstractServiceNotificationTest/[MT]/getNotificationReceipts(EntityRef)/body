{
  Results r=app.getEm().getCollection(notification,Notification.RECEIPTS_COLLECTION,null,1000000,Query.Level.REFS,false);
  List<EntityRef> list=new ArrayList<EntityRef>();
  PagingResultsIterator it=new PagingResultsIterator(r);
  while (it.hasNext()) {
    list.add((EntityRef)it.next());
  }
  return list;
}
