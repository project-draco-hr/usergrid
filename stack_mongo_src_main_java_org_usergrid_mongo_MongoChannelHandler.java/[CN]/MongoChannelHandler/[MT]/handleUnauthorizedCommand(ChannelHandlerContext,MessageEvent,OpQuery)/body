{
  OpReply reply=new OpReply(opQuery);
  reply.addDocument(map(entry("assertion","unauthorized db:" + opQuery.getDatabaseName() + " lock type:-1 client:"+ ((InetSocketAddress)e.getRemoteAddress()).getAddress().getHostAddress()),entry("assertionCode",10057),entry("errmsg","db assertion failure"),entry("ok",0.0)));
  return reply;
}
