{
  if (count <= 0) {
    count=DEFAULT_COUNT;
  }
  if (NULL_ID.equals(start)) {
    start=null;
  }
  final boolean skipFirst=start != null;
  IndexScanner scanner=new IndexBucketScanner(this,locator,ENTITY_ID_SETS,applicationId,IndexType.COLLECTION,key,start,finish,reversed,count,skipFirst,collectionName);
  return scanner;
}
