{
  if (count <= 0) {
    count=DEFAULT_COUNT;
  }
  if (NULL_ID.equals(start)) {
    start=null;
  }
  IndexScanner scanner=new IndexBucketScanner(this,locator,ENTITY_ID_SETS,applicationId,IndexType.COLLECTION,key,start,finish,reversed,Math.min(count * 10,1000),collectionName);
  return scanner;
}
