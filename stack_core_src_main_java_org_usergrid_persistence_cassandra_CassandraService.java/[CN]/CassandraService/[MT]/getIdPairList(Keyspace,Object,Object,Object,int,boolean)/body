{
  if (count <= 0) {
    count=DEFAULT_COUNT;
  }
  Map<UUID,UUID> ids=new LinkedHashMap<UUID,UUID>();
  if (NULL_ID.equals(start)) {
    start=null;
  }
  List<HColumn<ByteBuffer,ByteBuffer>> results=getColumns(ko,ENTITY_ID_SETS,key,start,finish,count,reversed);
  if (results != null) {
    for (    HColumn<ByteBuffer,ByteBuffer> result : results) {
      ids.put(uuid(result.getName()),uuid(result.getValue()));
    }
  }
  return ids;
}
