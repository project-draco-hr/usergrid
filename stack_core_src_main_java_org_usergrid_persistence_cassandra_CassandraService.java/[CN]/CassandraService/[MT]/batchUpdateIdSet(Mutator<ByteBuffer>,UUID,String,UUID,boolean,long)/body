{
  if (elementValue != null) {
    if (!removeFromSet) {
      addInsertToMutator(batch,ENTITY_ID_SETS,key(entityId,setName),elementValue,ByteBuffer.allocate(0),timestamp);
      addInsertToMutator(batch,ENTITY_DICTIONARIES,key(entityId,DICTIONARY_ID_SETS),setName,null,timestamp);
    }
 else {
      addDeleteToMutator(batch,ENTITY_ID_SETS,key(entityId,setName),timestamp);
    }
  }
  return batch;
}
