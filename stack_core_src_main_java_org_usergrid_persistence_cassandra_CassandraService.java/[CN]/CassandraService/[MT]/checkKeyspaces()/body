{
  List<KeyspaceDefinition> ksDefs=null;
  try {
    ksDefs=cluster.describeKeyspaces();
  }
 catch (  Exception e) {
    db_logger.error("Unable to describe keyspaces",e);
  }
  if (ksDefs != null) {
    for (    KeyspaceDefinition ksDef : ksDefs) {
      logger.info(ksDef.getName().toString());
    }
  }
}
