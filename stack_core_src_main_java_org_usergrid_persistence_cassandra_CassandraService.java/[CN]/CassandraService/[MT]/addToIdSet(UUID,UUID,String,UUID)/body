{
  if (elementValue == null) {
    return;
  }
  long timestamp=createTimestamp();
  Mutator<ByteBuffer> batch=createMutator(getApplicationKeyspace(applicationId),be);
  batch=batchUpdateIdSet(batch,entityId,setName,elementValue,false,timestamp);
  batchExecute(batch,CassandraService.RETRY_COUNT);
}
