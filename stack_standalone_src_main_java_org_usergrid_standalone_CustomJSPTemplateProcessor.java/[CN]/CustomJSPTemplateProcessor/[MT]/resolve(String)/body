{
  if (servletContext == null) {
    return null;
  }
  if (basePath != "") {
    path=basePath + path;
  }
  try {
    if (findJsp(path) != null) {
      return path;
    }
    if (!path.endsWith(".jsp")) {
      path=path + ".jsp";
      if (findJsp(path) != null) {
        return path;
      }
    }
  }
 catch (  MalformedURLException ex) {
  }
  return null;
}
