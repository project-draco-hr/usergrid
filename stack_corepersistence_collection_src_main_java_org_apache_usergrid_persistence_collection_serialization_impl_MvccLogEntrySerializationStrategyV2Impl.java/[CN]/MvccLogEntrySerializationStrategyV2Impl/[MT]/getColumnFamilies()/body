{
  MultiTenantColumnFamilyDefinition cf=new MultiTenantColumnFamilyDefinition(CF_ENTITY_LOG_V2,BytesType.class.getSimpleName(),ReversedType.class.getSimpleName() + "(" + UUIDType.class.getSimpleName()+ ")",IntegerType.class.getSimpleName(),MultiTenantColumnFamilyDefinition.CacheOption.KEYS);
