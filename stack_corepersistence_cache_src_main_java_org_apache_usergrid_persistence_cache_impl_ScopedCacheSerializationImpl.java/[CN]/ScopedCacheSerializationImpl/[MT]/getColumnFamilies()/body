{
  final MultiTenantColumnFamilyDefinition scopedCache=new MultiTenantColumnFamilyDefinition(SCOPED_CACHE,BytesType.class.getSimpleName(),BytesType.class.getSimpleName(),BytesType.class.getSimpleName(),MultiTenantColumnFamilyDefinition.CacheOption.KEYS);
  return Collections.singletonList(scopedCache);
}
