{
  if (rootOperand != null) {
    TreeEvaluator visitor=new TreeEvaluator();
    rootOperand.visit(visitor);
    rootNode=visitor.getRootNode();
  }
  if (sorts.size() > 0) {
    SliceNode sorts=generateSorts();
    if (rootNode != null) {
      AndNode and=new AndNode(sorts,rootNode);
      rootNode=and;
    }
 else {
      rootNode=sorts;
    }
  }
  if (rootNode == null) {
    rootNode=new AllNode(0);
  }
}
