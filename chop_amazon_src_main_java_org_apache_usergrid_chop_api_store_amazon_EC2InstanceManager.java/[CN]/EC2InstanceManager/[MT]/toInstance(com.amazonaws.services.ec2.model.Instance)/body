{
  Instance instance;
  BasicInstanceSpec spec;
  spec=new BasicInstanceSpec();
  spec.setImageId(ec2.getImageId());
  spec.setKeyName(ec2.getKeyName());
  spec.setType(ec2.getInstanceType());
  instance=new AmazonInstance(ec2.getInstanceId(),spec,InstanceState.fromValue(ec2.getState().getName()),ec2.getPrivateDnsName(),ec2.getPublicDnsName(),ec2.getPrivateIpAddress(),ec2.getPublicIpAddress());
  return instance;
}
