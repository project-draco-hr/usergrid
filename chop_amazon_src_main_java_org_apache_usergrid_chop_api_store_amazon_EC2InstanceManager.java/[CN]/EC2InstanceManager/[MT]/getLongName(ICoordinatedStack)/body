{
  StringBuilder sb=new StringBuilder();
  sb.append(stack.getUser().getUsername()).append("-").append(stack.getModule().getGroupId()).append("-").append(stack.getModule().getArtifactId()).append("-").append(stack.getModule().getVersion()).append("-").append(stack.getCommit().getId()).append("-").append(stack.getName());
  return sb.toString();
}
