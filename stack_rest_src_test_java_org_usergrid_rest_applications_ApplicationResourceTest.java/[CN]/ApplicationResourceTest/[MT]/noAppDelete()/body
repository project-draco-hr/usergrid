{
  String mgmtToken=adminToken();
  Status status=null;
  JsonNode node=null;
  try {
    node=resource().path("/test-organization/test-app").queryParam("access_token",mgmtToken).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).delete(JsonNode.class);
  }
 catch (  UniformInterfaceException uie) {
    status=uie.getResponse().getClientResponseStatus();
  }
  assertEquals(Status.BAD_REQUEST,status);
}
