{
  final ResultIterator iterator=visitor.getResults();
  if (!iterator.hasNext()) {
    return;
  }
  final Iterator<ScanColumn> nextPage=iterator.next().iterator();
  for (int i=0; i < pageSize && nextPage.hasNext(); i++) {
    results.add(nextPage.next());
    if (results.size() > pageSize) {
      results.pollLast();
    }
  }
}
