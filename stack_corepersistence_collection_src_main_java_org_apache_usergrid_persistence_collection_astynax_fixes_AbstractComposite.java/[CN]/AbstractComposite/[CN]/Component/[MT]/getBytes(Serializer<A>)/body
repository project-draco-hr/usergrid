{
  if (bytes == null) {
    if (value instanceof ByteBuffer) {
      return ((ByteBuffer)value).duplicate();
    }
    if (value == null) {
      return null;
    }
    if (s == null) {
      s=(Serializer<A>)serializer;
    }
    if (s != null) {
      return s.toByteBuffer((A)value).duplicate();
    }
  }
  return bytes.duplicate();
}
