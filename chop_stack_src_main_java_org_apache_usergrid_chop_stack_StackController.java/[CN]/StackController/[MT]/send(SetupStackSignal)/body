{
  Preconditions.checkState(state.accepts(signal),state.getMessage(signal));
switch (signal) {
case STOP:
    stop();
  break;
case START:
start();
break;
case RESET:
reset();
break;
case DESTROY:
destroy();
break;
case DEPLOY:
deploy();
break;
case SETUP:
setup();
break;
default :
throw new IllegalStateException("Just accepting start, stop, reset, destroy, deploy or setup.");
}
}
