{
  final long maxTime=(timeService.getCurrentTime() + graphFig.getShardCacheTimeout()) * 10000;
  for (  DirectedEdgeMeta.NodeMeta node : directedEdgeMeta.getNodes()) {
    final long uuidTime=node.getId().getUuid().timestamp();
    if (uuidTime < maxTime) {
      return true;
    }
  }
  return false;
}
