{
  logger.info("Initialize system keyspace");
  migrate();
  cass.createColumnFamily(getApplicationKeyspace(),createColumnFamilyDefinition(getApplicationKeyspace(),APPLICATIONS_CF,ComparatorType.BYTESTYPE));
  cass.createColumnFamily(getApplicationKeyspace(),createColumnFamilyDefinition(getApplicationKeyspace(),PROPERTIES_CF,ComparatorType.BYTESTYPE));
  cass.createColumnFamily(getApplicationKeyspace(),createColumnFamilyDefinition(getApplicationKeyspace(),TOKENS_CF,ComparatorType.BYTESTYPE));
  cass.createColumnFamily(getApplicationKeyspace(),createColumnFamilyDefinition(getApplicationKeyspace(),PRINCIPAL_TOKEN_CF,ComparatorType.UUIDTYPE));
  logger.info("System keyspace initialized");
}
