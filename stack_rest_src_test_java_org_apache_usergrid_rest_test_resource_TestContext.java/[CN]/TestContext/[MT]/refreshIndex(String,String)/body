{
  logger.debug("Refreshing index for app {}/{}",orgName,appName);
  try {
    root().resource().path("/refreshindex").queryParam("org_name",orgName).queryParam("app_name",appName).accept(MediaType.APPLICATION_JSON).post();
  }
 catch (  Exception e) {
    logger.debug("Error refreshing index",e);
    return;
  }
  logger.debug("Refreshed index for app {}/{}",orgName,appName);
}
