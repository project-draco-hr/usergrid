{
  try {
    UserInfo userInfo=validateAndReturnUserInfo(token,ttl);
    TokenInfo tokeninfo=new TokenInfo(UUIDUtils.newTimeUUID(),"access",1,1,1,ttl,new AuthPrincipalInfo(AuthPrincipalType.ADMIN_USER,userInfo.getUuid(),CpNamingUtils.MANAGEMENT_APPLICATION_ID),null);
    return tokeninfo;
  }
 catch (  Exception e) {
    logger.debug("Error construcing token info from userinfo");
    e.printStackTrace();
  }
  return null;
}
