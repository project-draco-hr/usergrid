{
  List<Entity> entity=null;
  EntityRef entityRef=new SimpleEntityRef(importEntityUUIDStr);
  try {
    entity=smf.getServiceManager(CpNamingUtils.MANAGEMENT_APPLICATION_ID).getEntityManager().getConnectedEntities(entityRef,"includes",null,Level.ALL_PROPERTIES).getEntities();
  }
 catch (  Exception e) {
    return Response.status(SC_BAD_REQUEST).type(JSONPUtils.jsonMediaType(callback)).entity(ServiceResource.wrapWithCallback(e.getMessage(),callback)).build();
  }
  if (entity == null) {
    return Response.status(SC_BAD_REQUEST).build();
  }
  return Response.status(SC_OK).entity(entity).build();
}
