{
  List<Entity> entity;
  try {
    entity=smf.getServiceManager(CassandraService.MANAGEMENT_APPLICATION_ID).getEntityManager().getConnectedEntities(importEntityUUIDStr,"includes",null,Results.Level.ALL_PROPERTIES).getEntities();
  }
 catch (  Exception e) {
    return Response.status(SC_BAD_REQUEST).type(JSONPUtils.jsonMediaType(callback)).entity(ServiceResource.wrapWithCallback(e.getMessage(),callback)).build();
  }
  if (entity == null) {
    return Response.status(SC_BAD_REQUEST).build();
  }
  return Response.status(SC_OK).entity(entity).build();
}
