{
  if (!recoverable) {
    return null;
  }
  if (!encrypted) {
    return secret;
  }
  if (isBlank(cipher)) {
    return secret;
  }
  if ("plaintext".equals(cipher)) {
    return secret;
  }
 else   if ("bcrypt".equals(cipher)) {
    return null;
  }
 else   if ("md5".equals(cipher)) {
    return null;
  }
 else   if ("sha-1".equals(cipher)) {
    return null;
  }
 else   if ("aes".equals(cipher)) {
    return decrypt(salt,secret);
  }
  return null;
}
