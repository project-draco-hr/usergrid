{
  Id sourceId=new SimpleId(source.getUuid(),source.getType());
  GraphManager gm=managerCache.getGraphManager(applicationScope);
  Observable<Edge> edgesFromSource=gm.loadEdgesFromSource(new SimpleSearchByEdgeType(sourceId,CpNamingUtils.getEdgeTypeFromConnectionType(connectionType),System.currentTimeMillis(),SearchByEdgeType.Order.DESCENDING,null));
  int count=edgesFromSource.take(2).count().toBlocking().last();
  return count > 1;
}
