{
  return HystrixGraphObservable.user(Observable.create(new ObservableIterator<MarkedEdge>("getEdgesToTarget"){
    @Override protected Iterator<MarkedEdge> getIterator(){
      return permanentStorageSerialization.getEdgesToTarget(scope,search);
    }
  }
).buffer(graphFig.getScanPageSize()).flatMap(new EdgeBufferFilter(search.getMaxVersion())).cast(Edge.class));
}
