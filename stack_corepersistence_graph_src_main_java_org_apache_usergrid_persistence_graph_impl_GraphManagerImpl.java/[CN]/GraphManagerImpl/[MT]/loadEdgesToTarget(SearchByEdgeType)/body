{
  return Observable.create(new ObservableIterator<MarkedEdge>(){
    @Override protected Iterator<MarkedEdge> getIterator(){
      return edgeSerialization.getEdgesToTarget(scope,search);
    }
  }
).buffer(graphFig.getScanPageSize()).flatMap(new EdgeBufferFilter(search.getMaxVersion())).cast(Edge.class);
}
