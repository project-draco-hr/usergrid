{
  return Observable.create(new ObservableIterator<MarkedEdge>(){
    @Override protected Iterator<MarkedEdge> getIterator(){
      return edgeSerialization.getEdgeVersions(scope,searchByEdge);
    }
  }
).buffer(graphFig.getScanPageSize()).flatMap(new EdgeBufferFilter(searchByEdge.getMaxVersion())).cast(Edge.class);
}
