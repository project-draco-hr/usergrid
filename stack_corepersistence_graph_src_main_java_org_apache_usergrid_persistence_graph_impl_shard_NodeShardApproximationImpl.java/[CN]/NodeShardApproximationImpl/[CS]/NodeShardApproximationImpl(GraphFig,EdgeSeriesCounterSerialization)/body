{
  this.graphFig=graphFig;
  this.edgeSeriesCounterSerialization=edgeSeriesCounterSerialization;
  graphLogs=CacheBuilder.newBuilder().maximumSize(graphFig.getShardCacheTimeout()).build(new CacheLoader<ShardKey,HyperLogLog>(){
    public HyperLogLog load(    ShardKey key){
      return loadCache(key);
    }
  }
);
}
