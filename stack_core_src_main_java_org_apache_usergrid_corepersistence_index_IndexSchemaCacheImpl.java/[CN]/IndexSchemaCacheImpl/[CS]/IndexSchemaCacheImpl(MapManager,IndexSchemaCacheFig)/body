{
  this.mapManager=mapManager;
  indexSchemaCache=CacheBuilder.newBuilder().maximumSize(indexSchemaCacheFig.getCacheSize()).build(new CacheLoader<String,Optional<String>>(){
    @Override public Optional<String> load(    final String collectionName) throws Exception {
      return Optional.ofNullable(retrieveCollectionSchema(collectionName));
    }
  }
);
}
