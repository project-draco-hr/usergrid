{
  String className=this.getClass().getSimpleName();
  Map<String,String> payload=hashMap("email",String.format("%s-%s@apigee.com",className,caller)).map("username",String.format("%s-%s-user",className,caller)).map("name",String.format("%s %s",className,caller)).map("password","password").map("organization",String.format("%s-%s-org",className,caller));
  return payload;
}
