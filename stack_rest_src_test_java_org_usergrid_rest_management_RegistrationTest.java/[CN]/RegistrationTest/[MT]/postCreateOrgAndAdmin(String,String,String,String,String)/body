{
  JsonNode node=null;
  Map<String,String> payload=hashMap("email",email).map("username",username).map("name",name).map("password",password).map("organization",organizationName);
  node=resource().path("/management/organizations").accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(JsonNode.class,payload);
  assertNotNull(node);
  logNode(node,LOG);
  return node;
}
