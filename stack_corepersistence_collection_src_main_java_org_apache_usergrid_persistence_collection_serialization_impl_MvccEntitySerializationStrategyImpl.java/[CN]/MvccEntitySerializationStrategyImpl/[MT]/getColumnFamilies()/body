{
  MultiTennantColumnFamilyDefinition cf=new MultiTennantColumnFamilyDefinition(columnFamily,BytesType.class.getSimpleName(),ReversedType.class.getSimpleName() + "(" + UUIDType.class.getSimpleName()+ ")",BytesType.class.getSimpleName(),MultiTennantColumnFamilyDefinition.CacheOption.KEYS);
  return Collections.singleton(cf);
}
