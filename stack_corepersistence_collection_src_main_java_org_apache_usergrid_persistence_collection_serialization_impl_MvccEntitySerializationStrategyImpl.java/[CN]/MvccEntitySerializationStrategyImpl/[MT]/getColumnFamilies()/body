{
  MultiTenantColumnFamilyDefinition cf=new MultiTenantColumnFamilyDefinition(columnFamily,BytesType.class.getSimpleName(),ReversedType.class.getSimpleName() + "(" + UUIDType.class.getSimpleName()+ ")",BytesType.class.getSimpleName(),MultiTenantColumnFamilyDefinition.CacheOption.KEYS);
  return Collections.singleton(cf);
}
