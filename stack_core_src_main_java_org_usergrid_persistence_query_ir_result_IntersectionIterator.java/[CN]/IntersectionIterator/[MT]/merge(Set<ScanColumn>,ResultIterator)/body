{
  Set<ScanColumn> results=new LinkedHashSet<ScanColumn>(pageSize);
  while (results.size() < pageSize) {
    if (!child.hasNext()) {
      child.reset();
      return results;
    }
    final Set<ScanColumn> childResults=child.next();
    results.addAll(Sets.intersection(current,childResults));
  }
  return results;
}
