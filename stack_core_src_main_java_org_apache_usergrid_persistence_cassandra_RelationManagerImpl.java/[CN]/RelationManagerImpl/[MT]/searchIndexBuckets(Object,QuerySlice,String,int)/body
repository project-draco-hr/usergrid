{
  DynamicComposite[] range=slice.getRange();
  Object keyPrefix=key(indexKey,slice.getPropertyName());
  IndexScanner scanner=new IndexBucketScanner(cass,indexBucketLocator,ENTITY_INDEX,applicationId,IndexType.COLLECTION,keyPrefix,range[0],range[1],slice.isReversed(),pageSize,slice.hasCursor(),collectionName);
  return scanner;
}
