{
  setUserPassword("ed@anuff.com","sesame");
  JsonNode node=mapper.readTree(resource().path("/test-organization/test-app/token").queryParam("grant_type","password").queryParam("username",name).queryParam("password",password).accept(MediaType.APPLICATION_JSON).get(String.class));
  String userToken=node.get("access_token").textValue();
  LOG.info("returning user token: {}",userToken);
  return userToken;
}
