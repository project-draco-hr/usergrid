{
  LOG.debug("Refreshing index for appId {}",appId);
  try {
    resource().path("/refreshindex").queryParam("app_id",appId.toString()).accept(MediaType.APPLICATION_JSON).post();
  }
 catch (  Exception e) {
    LOG.debug("Error refreshing index",e);
    return;
  }
  LOG.debug("Refreshed index for appId {}",appId);
}
