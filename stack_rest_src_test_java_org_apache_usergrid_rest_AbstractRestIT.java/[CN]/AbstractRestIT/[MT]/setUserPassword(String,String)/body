{
  Map<String,String> data=new HashMap<String,String>();
  data.put("newpassword",password);
  adminToken();
  JsonNode node=mapper.readTree(resource().path(String.format(orgAppPath + "users/%s/password",username)).queryParam("access_token",adminAccessToken).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(String.class,data));
}
