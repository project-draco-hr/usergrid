{
  Observable.create(new EdgesFromSourceObservable(applicationScope,sourceNode,gm)).doOnNext(new Action1<Edge>(){
    @Override public void call(    Edge edge){
      logger.info("Emitting targetId of  {}",edge);
      final Id targetNode=edge.getTargetNode();
      subscriber.onNext(targetNode);
    }
  }
).toBlocking().lastOrDefault(null);
}
