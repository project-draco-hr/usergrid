{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  final Shard shard=(Shard)o;
  if (compacted != shard.compacted) {
    return false;
  }
  if (createdTime != shard.createdTime) {
    return false;
  }
  if (shardIndex != shard.shardIndex) {
    return false;
  }
  return true;
}
