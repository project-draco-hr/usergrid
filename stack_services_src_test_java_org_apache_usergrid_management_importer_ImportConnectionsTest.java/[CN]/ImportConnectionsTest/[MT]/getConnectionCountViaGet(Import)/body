{
  try {
    EntityManager emMgmtApp=setup.getEmf().getEntityManager(CpNamingUtils.MANAGEMENT_APPLICATION_ID);
    Results entities=emMgmtApp.getConnectedEntities(importRoot,"includes",null,Query.Level.ALL_PROPERTIES);
    PagingResultsIterator itr=new PagingResultsIterator(entities);
    int count=0;
    while (itr.hasNext()) {
      itr.next();
      count++;
    }
    return count;
  }
 catch (  Exception e) {
    logger.error("application doesn't exist within the current context");
    throw new RuntimeException(e);
  }
}
