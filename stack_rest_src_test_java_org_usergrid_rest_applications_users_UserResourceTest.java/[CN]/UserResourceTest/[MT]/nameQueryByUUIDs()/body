{
  UserRepo.INSTANCE.load(resource(),access_token);
  String ql="name = 'John*'";
  ApplicationInfo appInfo=managementService.getApplicationInfo("test-organization/test-app");
  OrganizationInfo orgInfo=managementService.getOrganizationByName("test-organization");
  resource().path("/" + orgInfo.getUuid() + "/"+ appInfo.getId()+ "/users").queryParam("ql",ql).queryParam("access_token",access_token).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).get(JsonNode.class);
}
