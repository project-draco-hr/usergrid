{
  JsonNode response=resource.path("/test-app/users").queryParam("access_token",access_token).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(JsonNode.class,payload);
  String idString=response.get("entities").get(0).get("uuid").asText();
  return UUIDUtils.tryExtractUUID(idString);
}
