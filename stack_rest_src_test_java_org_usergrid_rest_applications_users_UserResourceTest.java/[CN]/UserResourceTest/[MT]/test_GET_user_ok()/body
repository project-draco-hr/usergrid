{
  JsonNode node=resource().path("/test-app/users").queryParam("access_token",access_token).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).get(JsonNode.class);
  String uuid=node.get("entities").get(0).get("uuid").getTextValue();
  node=resource().path("/test-app/users/" + uuid).queryParam("access_token",access_token).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).get(JsonNode.class);
  logNode(node);
  assertEquals("ed@anuff.com",node.get("entities").get(0).get("email").getTextValue());
}
