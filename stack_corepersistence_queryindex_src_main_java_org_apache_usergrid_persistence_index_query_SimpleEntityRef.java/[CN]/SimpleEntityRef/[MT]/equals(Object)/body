{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  SimpleEntityRef other=(SimpleEntityRef)obj;
  if (id == null) {
    if (other.id != null) {
      return false;
    }
  }
 else   if (!id.equals(other.id)) {
    return false;
  }
  if (version == null) {
    if (other.version != null) {
      return false;
    }
  }
 else   if (!version.equals(other.version)) {
    return false;
  }
  return true;
}
