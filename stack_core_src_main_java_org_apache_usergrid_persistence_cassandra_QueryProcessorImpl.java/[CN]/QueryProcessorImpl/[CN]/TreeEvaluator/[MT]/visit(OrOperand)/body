{
  Operand left=op.getLeft();
  Operand right=op.getRight();
  createNewSlice(left);
  left.visit(this);
  createNewSlice(right);
  right.visit(this);
  QueryNode rightResult=nodes.pop();
  QueryNode leftResult=nodes.pop();
  OrNode orNode=new OrNode(leftResult,rightResult,++contextCount);
  nodes.push(orNode);
}
