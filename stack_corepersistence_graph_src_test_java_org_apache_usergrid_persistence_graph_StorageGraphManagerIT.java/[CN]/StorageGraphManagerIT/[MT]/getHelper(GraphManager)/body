{
  final ComittedGraphTestHelper helper=new ComittedGraphTestHelper(gm);
  edgeDelete.addCompleteListener(new CompleteListener<EdgeEvent<Edge>>(){
    @Override public void onComplete(    final AsynchronousMessage<EdgeEvent<Edge>> event){
      helper.complete();
    }
  }
);
  edgeDelete.addErrorListener(new ErrorListener<EdgeEvent<Edge>>(){
    @Override public void onError(    final AsynchronousMessage<EdgeEvent<Edge>> event,    final Throwable t){
      helper.complete();
      helper.error();
    }
  }
);
  nodeDelete.addCompleteListener(new CompleteListener<EdgeEvent<Id>>(){
    @Override public void onComplete(    final AsynchronousMessage<EdgeEvent<Id>> event){
      helper.complete();
    }
  }
);
  nodeDelete.addErrorListener(new ErrorListener<EdgeEvent<Id>>(){
    @Override public void onError(    final AsynchronousMessage<EdgeEvent<Id>> event,    final Throwable t){
      helper.complete();
      helper.error();
    }
  }
);
  edgeWrite.addCompleteListener(new CompleteListener<EdgeEvent<Edge>>(){
    @Override public void onComplete(    final AsynchronousMessage<EdgeEvent<Edge>> event){
      helper.complete();
    }
  }
);
  edgeWrite.addErrorListener(new ErrorListener<EdgeEvent<Edge>>(){
    @Override public void onError(    final AsynchronousMessage<EdgeEvent<Edge>> event,    final Throwable t){
      helper.complete();
      helper.error();
    }
  }
);
  return helper;
}
