{
  if (flushPending == null && (currentCounter.getCreateTimestamp() + graphFig.getCounterFlushInterval() > timeService.getCurrentTime() || currentCounter.getInvokeCount() >= graphFig.getCounterFlushCount())) {
    Schedulers.immediate().createWorker().schedule(new Action0(){
      @Override public void call(){
        flush();
      }
    }
);
  }
}
