{
  QuerySlice slice=node.getSlice();
  queryProcessor.applyCursorAndSort(slice);
  final int size=queryProcessor.getPageSizeHint(node);
  GeoIterator itr=new GeoIterator(new ConnectionGeoSearch(em,bucket,cassandraService,connection.getIndexId()),size,slice,node.getPropertyName(),new Point(node.getLattitude(),node.getLongitude()),node.getDistance());
  this.results.push(itr);
}
