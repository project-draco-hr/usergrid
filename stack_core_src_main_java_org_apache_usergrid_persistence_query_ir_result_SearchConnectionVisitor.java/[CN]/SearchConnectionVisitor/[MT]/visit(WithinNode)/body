{
  QuerySlice slice=node.getSlice();
  queryProcessor.applyCursorAndSort(slice);
  GeoIterator itr=new GeoIterator(new ConnectionGeoSearch(em,indexBucketLocator,cassandraService,connection.getIndexId()),query.getLimit(),slice,node.getPropertyName(),new Point(node.getLattitude(),node.getLongitude()),node.getDistance());
  results.push(itr);
}
