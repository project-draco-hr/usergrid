{
  DynamicComposite[] range=slice.getRange();
  Object keyPrefix=key(indexKey,slice.getPropertyName());
  IndexScanner scanner=new IndexBucketScanner(cassandraService,ENTITY_INDEX,DynamicCompositeStartToBytes.INSTANCE,applicationId,keyPrefix,shardBucket,range[0],range[1],slice.isReversed(),pageSize,slice.hasCursor());
  return scanner;
}
