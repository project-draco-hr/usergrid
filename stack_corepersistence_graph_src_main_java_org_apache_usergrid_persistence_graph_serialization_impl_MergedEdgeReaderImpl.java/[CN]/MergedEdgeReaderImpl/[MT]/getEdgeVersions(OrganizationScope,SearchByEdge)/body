{
  Observable<MarkedEdge> commitLog=Observable.create(new ObservableIterator<MarkedEdge>("ggetEdgeVersionsCommitLog"){
    @Override protected Iterator<MarkedEdge> getIterator(){
      return commitLogSerialization.getEdgeVersions(scope,search);
    }
  }
);
  Observable<MarkedEdge> permanent=Observable.create(new ObservableIterator<MarkedEdge>("getEdgeVersionsStorage"){
    @Override protected Iterator<MarkedEdge> getIterator(){
      return permanentSerialization.getEdgeVersions(scope,search);
    }
  }
);
  return OrderedMerge.orderedMerge(EdgeComparator.INSTANCE,cassandraConfig.getScanPageSize() / 2,commitLog,permanent).distinctUntilChanged();
}
