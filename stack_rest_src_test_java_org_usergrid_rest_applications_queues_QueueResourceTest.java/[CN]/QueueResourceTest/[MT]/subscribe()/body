{
  TestAdminUser testAdmin=new TestAdminUser("queueresourcetest.subscribe","queueresourcetest.subscribe@usergrid.com","queueresourcetest.subscribe@usergrid.com");
  TestContext context=TestContext.create(this).withOrg("queueresourcetest.subscribe").withApp("subscribe").withUser(testAdmin).initAll();
  Queue queue=context.application().queues().queue("test");
  queue.subscribers().subscribe("testsub1");
  queue.subscribers().subscribe("testsub2");
  final int count=30;
  for (int i=0; i < count; i++) {
    queue.post(MapUtils.hashMap("id",i));
  }
  testMessages(queue,count,null);
  queue=context.application().queues().queue("testsub1");
  testMessages(queue,count,null);
  queue=context.application().queues().queue("testsub2");
  testMessages(queue,count,null);
}
