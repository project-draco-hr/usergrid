{
  startSpring();
  setVerbose(line);
  prepareBaseOutputFileName(line);
  exportDir=createOutputParentDir();
  logger.info("Export directory: " + exportDir.getAbsolutePath());
  exportOrganizations();
  BiMap<UUID,String> organizations=managementService.getOrganizations();
  for (  Entry<UUID,String> organization : organizations.entrySet()) {
    if (organization.equals(properties.getProperty("usergrid.test-account.organization"))) {
      continue;
    }
    exportApplicationsForOrg(organization);
  }
}
