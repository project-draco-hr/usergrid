{
  if (message instanceof UniqueValueActor.Request) {
    UniqueValueActor.Request request=(UniqueValueActor.Request)message;
    ConsistentHashingRouter.ConsistentHashableEnvelope envelope=new ConsistentHashingRouter.ConsistentHashableEnvelope(message,request.getRowKey());
    router.tell(envelope,getSender());
  }
 else {
    unhandled(message);
  }
}
