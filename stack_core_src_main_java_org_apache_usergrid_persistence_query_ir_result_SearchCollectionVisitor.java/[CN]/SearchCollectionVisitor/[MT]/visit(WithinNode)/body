{
  QuerySlice slice=node.getSlice();
  queryProcessor.applyCursorAndSort(slice);
  final int size=queryProcessor.getPageSizeHint(node);
  GeoIterator itr=new GeoIterator(new CollectionGeoSearch(em,indexBucketLocator,cassandraService,headEntity,collection.getName()),size,slice,node.getPropertyName(),new Point(node.getLattitude(),node.getLongitude()),node.getDistance());
  final CollectionShardFilter validator=new CollectionShardFilter(indexBucketLocator,bucket);
  this.results.push(new ShardFilterIterator(validator,itr,size));
}
