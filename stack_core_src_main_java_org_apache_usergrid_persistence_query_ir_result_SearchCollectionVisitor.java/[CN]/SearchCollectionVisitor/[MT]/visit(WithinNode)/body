{
  QuerySlice slice=node.getSlice();
  queryProcessor.applyCursorAndSort(slice);
  GeoIterator itr=new GeoIterator(new CollectionGeoSearch(em,indexBucketLocator,cassandraService,headEntity,collection.getName()),query.getLimit(),slice,node.getPropertyName(),new Point(node.getLattitude(),node.getLongitude()),node.getDistance());
  results.push(itr);
}
