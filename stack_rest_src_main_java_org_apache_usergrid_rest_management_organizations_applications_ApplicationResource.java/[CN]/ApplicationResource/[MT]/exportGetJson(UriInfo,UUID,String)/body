{
  Export entity;
  try {
    entity=smf.getServiceManager(applicationId).getEntityManager().get(jobUUIDStr,Export.class);
  }
 catch (  Exception e) {
    OAuthResponse errorMsg=OAuthResponse.errorResponse(SC_BAD_REQUEST).setErrorDescription(e.getMessage()).buildJSONMessage();
    return Response.status(errorMsg.getResponseStatus()).type(JSONPUtils.jsonMediaType(callback)).entity(ServiceResource.wrapWithCallback(errorMsg.getBody(),callback)).build();
  }
  if (entity == null) {
    return Response.status(SC_BAD_REQUEST).build();
  }
  return Response.status(SC_OK).entity(entity.getState()).build();
}
