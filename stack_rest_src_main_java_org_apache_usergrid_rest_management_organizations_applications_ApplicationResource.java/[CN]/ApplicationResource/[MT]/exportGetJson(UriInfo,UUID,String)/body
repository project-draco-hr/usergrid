{
  Export entity;
  try {
    entity=smf.getServiceManager(applicationId).getEntityManager().get(jobUUIDStr,Export.class);
  }
 catch (  Exception e) {
    return Response.status(SC_BAD_REQUEST).build();
  }
  if (entity == null) {
    return Response.status(SC_BAD_REQUEST).build();
  }
  return Response.status(SC_OK).entity(entity.getState()).build();
}
