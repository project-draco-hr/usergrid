{
  Export entity;
  try {
    entity=smf.getServiceManager(applicationId).getEntityManager().get(jobUUIDStr,Export.class);
  }
 catch (  Exception e) {
    return Response.status(SC_BAD_REQUEST).type(JSONPUtils.jsonMediaType(callback)).entity(ServiceResource.wrapWithCallback(e.getMessage(),callback)).build();
  }
  if (entity == null) {
    return Response.status(SC_BAD_REQUEST).build();
  }
  return Response.status(SC_OK).entity(entity.getState()).build();
}
