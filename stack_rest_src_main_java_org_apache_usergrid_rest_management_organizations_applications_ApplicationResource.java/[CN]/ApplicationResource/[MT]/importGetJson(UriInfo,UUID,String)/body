{
  Import entity;
  try {
    entity=smf.getServiceManager(CpNamingUtils.MANAGEMENT_APPLICATION_ID).getEntityManager().get(importEntityUUIDStr,Import.class);
  }
 catch (  Exception e) {
    return Response.status(SC_BAD_REQUEST).type(JSONPUtils.jsonMediaType(callback)).entity(ServiceResource.wrapWithCallback(e.getMessage(),callback)).build();
  }
  if (entity == null) {
    return Response.status(SC_NOT_FOUND).type(JSONPUtils.jsonMediaType(callback)).entity(ServiceResource.wrapWithCallback("Import Job not found",callback)).build();
  }
  return Response.status(SC_OK).entity(entity).build();
}
