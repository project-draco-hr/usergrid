{
  init();
  EntityManager em=getEntityManager(NamingUtils.SYSTEM_APP_ID);
  final EntityRef alias=em.getAlias("appinfos",name);
  if (alias == null) {
    return null;
  }
  final Entity entity=em.get(alias);
  if (entity == null) {
    return null;
  }
  final UUID property=(UUID)entity.getProperty("applicationUuid");
  return property;
}
