{
  Query q=Query.fromQL(PROPERTY_NAME + " = '" + name+ "'");
  EntityIndex ei=getManagerCache().getEntityIndex(SYSTEM_ORG_SCOPE,SYSTEM_APPS_SCOPE);
  org.apache.usergrid.persistence.index.query.Results results=ei.search(SYSTEM_APPS_SCOPE,q);
  if (results.isEmpty()) {
    return null;
  }
  return results.iterator().next().getId().getUuid();
}
