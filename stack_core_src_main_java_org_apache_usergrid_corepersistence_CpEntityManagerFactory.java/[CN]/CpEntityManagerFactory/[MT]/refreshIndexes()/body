{
  EntityIndex aei=managerCache.getEntityIndex(CpEntityManagerFactory.SYSTEM_APPS_INDEX_SCOPE);
  EntityIndex oei=managerCache.getEntityIndex(CpEntityManagerFactory.SYSTEM_ORGS_INDEX_SCOPE);
  EntityIndex pei=managerCache.getEntityIndex(CpEntityManagerFactory.SYSTEM_PROPS_INDEX_SCOPE);
  aei.refresh();
  oei.refresh();
  pei.refresh();
}
