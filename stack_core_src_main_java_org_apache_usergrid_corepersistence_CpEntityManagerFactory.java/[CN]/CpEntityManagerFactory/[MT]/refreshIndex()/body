{
  managerCache.getEntityIndex(new ApplicationScopeImpl(new SimpleId(SYSTEM_APP_ID,"application"))).refresh();
  managerCache.getEntityIndex(new ApplicationScopeImpl(new SimpleId(getManagementAppId(),"application"))).refresh();
  managerCache.getEntityIndex(new ApplicationScopeImpl(new SimpleId(getDefaultAppId(),"application"))).refresh();
}
