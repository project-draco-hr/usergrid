{
  CollectionScope collScope=new CollectionScopeImpl(getApplicationScope(SYSTEM_APP_ID).getApplication(),getApplicationScope(SYSTEM_APP_ID).getApplication(),CpNamingUtils.getCollectionScopeNameFromCollectionName("appinfos"));
