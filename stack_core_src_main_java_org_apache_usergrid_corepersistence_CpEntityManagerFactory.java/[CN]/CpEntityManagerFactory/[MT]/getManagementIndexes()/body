{
  return Arrays.asList(getManagerCache().getEntityIndex(new ApplicationScopeImpl(new SimpleId(SYSTEM_APP_ID,"application"))),getManagerCache().getEntityIndex(new ApplicationScopeImpl(new SimpleId(getManagementAppId(),"application"))),getManagerCache().getEntityIndex(new ApplicationScopeImpl(new SimpleId(getDefaultAppId(),"application"))));
}
