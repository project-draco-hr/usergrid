{
  return Arrays.asList(managerCache.getEntityIndex(new ApplicationScopeImpl(new SimpleId(CpNamingUtils.SYSTEM_APP_ID,"application"))),managerCache.getEntityIndex(new ApplicationScopeImpl(new SimpleId(getManagementAppId(),"application"))),managerCache.getEntityIndex(new ApplicationScopeImpl(new SimpleId(getDefaultAppId(),"application"))));
}
