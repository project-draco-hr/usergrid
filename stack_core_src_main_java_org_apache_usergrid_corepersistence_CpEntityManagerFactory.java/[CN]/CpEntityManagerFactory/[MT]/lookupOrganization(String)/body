{
  init();
  EntityManager em=getEntityManager(CpNamingUtils.MANAGEMENT_APPLICATION_ID);
  final EntityRef alias=em.getAlias("organizations",name);
  if (alias == null) {
    return null;
  }
  final Entity entity=em.get(alias);
  if (entity == null) {
    return null;
  }
  return entity.getUuid();
}
