{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  AbstractEntity other=(AbstractEntity)obj;
  if (uuid == null) {
    if (other.uuid != null)     return false;
  }
 else   if (!uuid.equals(other.uuid))   return false;
  return true;
}
