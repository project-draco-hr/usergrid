{
  logger.info("NOTE: ImportServiceIT tests will not run unless secretKey, accessKey and bucketName " + "are specified as system properties. You can do this in your Maven settings.xml file.");
  Assume.assumeTrue(StringUtils.isEmpty(System.getProperty("secretKey")) && StringUtils.isEmpty(System.getProperty("accessKey")) && StringUtils.isEmpty(System.getProperty("bucketName")));
}
