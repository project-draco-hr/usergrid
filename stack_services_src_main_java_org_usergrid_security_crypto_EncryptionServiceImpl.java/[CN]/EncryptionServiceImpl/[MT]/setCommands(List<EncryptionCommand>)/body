{
  for (  EncryptionCommand command : inputCommands) {
    String name=command.getName();
    Assert.notNull(name,"Encryption command name cannot be null");
    EncryptionCommand existing=commands.get(name);
    Assert.isNull(existing,String.format("Both class %s and %s implement command %s.  This is a wiring bug, and not allowed",command.getClass().getName(),existing.getClass().getName(),name));
    commands.put(name,command);
  }
}
