{
  if (tokens.isExternalSSOProviderEnabled()) {
    throw new IllegalArgumentException("Admin Users must reactivate via " + properties.getProperty(USERGRID_EXTERNAL_SSO_PROVIDER_URL));
  }
  logger.info("Send activation email for user: {}",user.getUuid());
  ApiResponse response=createApiResponse();
  management.startAdminUserActivationFlow(null,user);
  response.setAction("reactivate user");
  return response;
}
