{
  Object property_index_key=key(index_keys[ConnectionRefImpl.ALL],INDEX_CONNECTIONS,propertyName,DICTIONARY_GEOCELL,geoCell,locator.getBucket(appId,IndexType.GEO,index_keys[ConnectionRefImpl.ALL],geoCell));
  Object entity_type_prop_index_key=key(index_keys[ConnectionRefImpl.BY_ENTITY_TYPE],INDEX_CONNECTIONS,propertyName,DICTIONARY_GEOCELL,geoCell,locator.getBucket(appId,IndexType.GEO,index_keys[ConnectionRefImpl.BY_ENTITY_TYPE],geoCell));
  Object connection_type_prop_index_key=key(index_keys[ConnectionRefImpl.BY_CONNECTION_TYPE],INDEX_CONNECTIONS,propertyName,DICTIONARY_GEOCELL,geoCell,locator.getBucket(appId,IndexType.GEO,index_keys[ConnectionRefImpl.BY_CONNECTION_TYPE],geoCell));
