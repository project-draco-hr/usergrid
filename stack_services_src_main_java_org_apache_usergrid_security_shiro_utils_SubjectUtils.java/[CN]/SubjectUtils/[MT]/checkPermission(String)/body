{
  Subject currentUser=getSubject();
  if (currentUser == null) {
    return;
  }
  try {
    currentUser.checkPermission(permission);
  }
 catch (  org.apache.shiro.authz.UnauthenticatedException e) {
    if (logger.isDebugEnabled()) {
      logger.debug("checkPermission(): Subject is anonymous");
    }
  }
}
