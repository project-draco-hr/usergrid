{
  return resource.queryParam(RUNNER_HOSTNAME,runner.getHostname()).queryParam(RUNNER_PORT,String.valueOf(runner.getServerPort())).queryParam(RUNNER_IPV4_ADDRESS,runner.getIpv4Address()).queryParam(MODULE_GROUPID,project.getGroupId()).queryParam(MODULE_ARTIFACTID,project.getArtifactId()).queryParam(MODULE_VERSION,project.getVersion()).queryParam(COMMIT_ID,project.getVcsVersion()).queryParam(USERNAME,coordinatorFig.getUsername()).queryParam(PASSWORD,coordinatorFig.getPassword());
}
