{
  super(pageSize);
  this.slice=slice;
  this.firstOrder=firstOrder;
  this.parser=parser;
  subSortCompare=new ComparatorChain();
  for (  SortPredicate sort : secondary) {
    subSortCompare.addComparator(new EntityPropertyComparator(sort.getPropertyName(),sort.getDirection() == Query.SortDirection.DESCENDING));
  }
}
