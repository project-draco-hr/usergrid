{
  if (!isRootService()) {
    return null;
  }
  String nameProperty=Schema.getDefaultSchema().aliasProperty(getEntityType());
  if (nameProperty == null) {
    nameProperty="name";
  }
  Entity entity=em.getUniqueEntityFromAlias(getEntityType(),name,true);
  if (entity != null) {
    entity=importEntity(request,entity);
  }
  return entity;
}
