{
  EntityRef entity=null;
  if (!context.moreParameters()) {
    entity=em.get(new SimpleEntityRef(getEntityType(),id));
    entity=importEntity(context,(Entity)entity);
  }
 else {
    entity=em.get(new SimpleEntityRef(getEntityType(),id));
  }
  if (entity == null) {
    throw new ServiceResourceNotFoundException(context);
  }
  return entity;
}
