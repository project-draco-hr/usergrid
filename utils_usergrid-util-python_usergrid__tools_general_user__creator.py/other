import json
import requests
users = ['me@example.com']
for user in users:
    post_body = {'username': user, 'name': user, 'email': user, 'password': 'test12345', }
    print json.dumps(post_body)
    r = requests.post('http://localhost:8080/management/organizations/asdf/users', headers={'Authorization': 'Bearer SADFSDF', 'Content-Type': 'application/json', }, data=json.dumps(post_body))
    print r.status_code
    print ('%s: created (POST) [%s]: %s' % (user, r.status_code, r.text))
