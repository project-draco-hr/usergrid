{
  this.value=value.trim().toLowerCase();
  if ("*".equals(value)) {
    this.value=null;
    this.finishValue=null;
    return;
  }
  if (value != null && value.endsWith("*")) {
    this.value=removeEnd(value.toString(),"*");
    finishValue=this.value + "\uFFFF";
  }
 else {
    finishValue=value;
  }
}
