{
  super("Unable to convert property \"" + propertyName + "\" of entity \""+ entityType+ "\" from value of type "+ propertyValue.getClass()+ " to value of type "+ propertyType,cause);
  this.entityType=entityType;
  this.propertyName=propertyName;
  this.propertyValue=propertyValue;
  this.propertyType=propertyType;
}
