{
  if (count == max) {
    return false;
  }
  if (count % pageSize == 0 || lastResults == null || !lastResults.hasNext()) {
    try {
      return load();
    }
 catch (    Exception e) {
      throw new RuntimeException("Error loading next page of indexbucket scanner",e);
    }
  }
  return lastResults.hasNext();
}
