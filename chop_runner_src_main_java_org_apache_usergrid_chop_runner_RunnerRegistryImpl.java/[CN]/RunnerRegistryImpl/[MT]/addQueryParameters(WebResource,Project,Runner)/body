{
  return resource.queryParam(RestParams.RUNNER_HOSTNAME,runner.getHostname()).queryParam(RestParams.RUNNER_PORT,String.valueOf(runner.getServerPort())).queryParam(RestParams.RUNNER_IPV4_ADDRESS,runner.getIpv4Address()).queryParam(RestParams.MODULE_GROUPID,project.getGroupId()).queryParam(RestParams.MODULE_ARTIFACTID,project.getArtifactId()).queryParam(RestParams.MODULE_VERSION,project.getVersion()).queryParam(RestParams.COMMIT_ID,project.getVcsVersion()).queryParam(RestParams.VCS_REPO_URL,project.getVcsRepoUrl()).queryParam(RestParams.USERNAME,coordinatorFig.getUsername()).queryParam(RestParams.PASSWORD,coordinatorFig.getPassword());
}
