{
  final BiMap<UUID,String> orgs=task.orgNamesByUuid;
  jg.writeFieldName("organizations");
  jg.writeStartArray();
  for (  UUID uuid : orgs.keySet()) {
    jg.writeStartObject();
    jg.writeFieldName("uuid");
    jg.writeObject(uuid);
    jg.writeFieldName("name");
    jg.writeObject(orgs.get(uuid));
    jg.writeEndObject();
  }
  jg.writeEndArray();
}
