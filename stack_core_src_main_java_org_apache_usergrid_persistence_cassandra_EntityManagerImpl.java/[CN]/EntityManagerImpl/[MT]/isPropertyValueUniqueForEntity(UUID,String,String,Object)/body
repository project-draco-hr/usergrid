{
  if (!getDefaultSchema().isPropertyUnique(entityType,propertyName)) {
    return true;
  }
  if (propertyValue == null) {
    return true;
  }
  Set<UUID> ownerEntityIds=getUUIDsForUniqueProperty(new SimpleEntityRef(Application.ENTITY_TYPE,applicationId),entityType,propertyName,propertyValue);
  return ownerEntityIds.size() == 0 || ownerEntityIds.contains(ownerEntityId);
}
