{
  String collectionNameInternal=defaultCollectionName(collectionName);
  Object key=createUniqueIndexKey(ownerEntityRef.getUuid(),collectionNameInternal,propertyName,propertyValue);
  List<HColumn<ByteBuffer,ByteBuffer>> cols=cass.getColumns(cass.getApplicationKeyspace(applicationId),ENTITY_UNIQUE,key,null,null,2,false);
  if (cols.size() == 0) {
    return Collections.emptySet();
  }
  if (cols.size() > 1) {
    logger.error("INDEX CORRUPTION: More than 1 unique value exists for entities in " + "ownerId {} of type {} on property {} with value {}",new Object[]{ownerEntityRef,collectionNameInternal,propertyName,propertyValue});
  }
  Set<UUID> results=new HashSet<UUID>(cols.size());
  for (  HColumn<ByteBuffer,ByteBuffer> col : cols) {
    results.add(ue.fromByteBuffer(col.getName()));
  }
  return results;
}
