{
  IndexLocationStrategy indexLocationStrategy=indexLocationStrategyFactory.getIndexLocationStrategy(applicationScope);
  String applicationId=applicationScope.getApplication().getUuid().toString().toLowerCase();
  assertEquals(indexLocationStrategy.getAlias().getReadAlias(),clusterName + "_" + keyspacename+ "_"+ applicationId+ "_read_"+ indexFig.getAliasPostfix());
  assertEquals(indexLocationStrategy.getAlias().getWriteAlias(),clusterName + "_" + keyspacename+ "_"+ applicationId+ "_write_"+ indexFig.getAliasPostfix());
}
