{
  assertAndQuery("select * where a = 1 and b = 2 and c = 3");
  assertAndQuery("select * where a = 1 AND b = 2 and c = 3");
  assertAndQuery("select * where a = 1 AnD b = 2 and c = 3");
  assertAndQuery("select * where a = 1 ANd b = 2 and c = 3");
  assertAndQuery("select * where a = 1 anD b = 2 and c = 3");
  assertAndQuery("select * where a = 1 ANd b = 2 and c = 3");
  assertAndQuery("select * where a = 1 && b = 2 && c = 3");
}
