{
  JsonNode userdata=mapper.readTree(resource().path("/management/users/test@usergrid.com/feed").queryParam("access_token",adminAccessToken).accept(MediaType.APPLICATION_JSON).get(String.class));
  assertTrue(StringUtils.contains(this.getEntity(userdata,0).get("title").asText(),"<a href=\"mailto:test@usergrid.com\">"));
}
