{
  Map<String,Object> payload=new LinkedHashMap<String,Object>();
  payload.put("username",username);
  resource().path("" + orgInfo.getName() + "/"+ appInfo.getName()+ "/users").queryParam("access_token",this.adminToken()).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(String.class,payload);
}
