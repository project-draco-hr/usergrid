{
  Map<String,Object> payload=new LinkedHashMap<String,Object>();
  payload.put("username",username);
  resource().path("" + context.getOrgName() + "/"+ context.getAppName()+ "/users").queryParam("access_token",context.getActiveUser().getToken()).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(String.class,payload);
}
