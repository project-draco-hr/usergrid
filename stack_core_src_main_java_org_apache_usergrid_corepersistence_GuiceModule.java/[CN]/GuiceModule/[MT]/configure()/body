{
  install(new CommonModule(){
    @Override protected void bindTimeoutQueueFactory(){
      bind(TimeoutQueueFactory.class).to(LocalTimeoutQueueFactory.class);
    }
  }
);
  install(new CollectionModule());
  install(new GraphModule());
  install(new IndexModule());
  bind(CpEntityDeleteListener.class).asEagerSingleton();
  bind(CpEntityIndexDeleteListener.class).asEagerSingleton();
}
