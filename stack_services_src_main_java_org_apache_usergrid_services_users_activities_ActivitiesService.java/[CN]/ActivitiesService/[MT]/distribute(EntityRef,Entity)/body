{
  if (activity == null) {
    return;
  }
  em.addToCollection(user,"feed",activity);
  Results r=em.getConnectingEntities(user.getUuid(),"following",User.ENTITY_TYPE,Results.Level.REFS,1000);
  List<EntityRef> refs=r.getRefs();
  if (refs != null) {
    em.addToCollections(refs,"feed",activity);
  }
}
