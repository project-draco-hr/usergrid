{
  IndexOperationMessage batch=new IndexOperationMessage();
  for (  EdgeScope e : edges) {
    EntityIndexOperation entityIndexOperation=new EntityIndexOperation(e.getApplicationScope(),e.getEdge().getTargetNode(),updatedSince);
    IndexOperationMessage indexOperationMessage=eventBuilder.buildEntityIndex(entityIndexOperation).toBlocking().lastOrDefault(null);
    if (indexOperationMessage != null) {
      batch.ingest(indexOperationMessage);
    }
  }
  queueIndexOperationMessage(batch);
}
