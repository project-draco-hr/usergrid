{
  StringBuilder sb=new StringBuilder();
  idString(sb,applicationScope.getApplication());
  sb.append(FIELD_SEPERATOR);
  idString(sb,scope.getNodeId());
  sb.append(FIELD_SEPERATOR);
  sb.append(scope.getEdgeName());
  sb.append(FIELD_SEPERATOR);
  sb.append(scope.getNodeType());
  return sb.toString();
}
