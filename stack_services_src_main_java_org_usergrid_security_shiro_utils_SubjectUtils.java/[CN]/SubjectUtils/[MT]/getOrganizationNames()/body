{
  Subject currentUser=SecurityUtils.getSubject();
  if (currentUser == null) {
    return null;
  }
  if (!currentUser.hasRole(ROLE_ORGANIZATION_ADMIN)) {
    return null;
  }
  BiMap<UUID,String> organizations=getOrganizations();
  if (organizations == null) {
    return null;
  }
  return organizations.inverse().keySet();
}
