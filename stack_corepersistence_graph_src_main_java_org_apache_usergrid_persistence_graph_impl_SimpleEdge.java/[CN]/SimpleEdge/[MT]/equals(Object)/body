{
  if (this == o) {
    return true;
  }
  if (!(o instanceof SimpleEdge)) {
    return false;
  }
  final Edge that=(Edge)o;
  if (!sourceNode.equals(that.getSourceNode())) {
    return false;
  }
  if (!targetNode.equals(that.getTargetNode())) {
    return false;
  }
  if (!type.equals(that.getType())) {
    return false;
  }
  if (!version.equals(that.getTimestamp())) {
    return false;
  }
  return true;
}
