{
  for (  final Notifier notifier : notifiers) {
    INACTIVE_DEVICE_CHECK_POOL.execute(new Runnable(){
      @Override public void run(){
        try {
          checkForInactiveDevices(notifier);
        }
 catch (        Exception e) {
          LOG.error("checkForInactiveDevices",e);
        }
      }
    }
);
  }
}
