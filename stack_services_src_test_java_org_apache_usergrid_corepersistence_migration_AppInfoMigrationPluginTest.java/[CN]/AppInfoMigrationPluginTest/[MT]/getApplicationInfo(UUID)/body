{
  UUID mgmtAppId=setup.getEmf().getManagementAppId();
  EntityManager rootEm=setup.getEmf().getEntityManager(mgmtAppId);
  final Results applicationInfoResults=rootEm.searchCollection(new SimpleEntityRef("application",mgmtAppId),"application_infos",Query.fromQL("select * where applicationId=" + appId.toString()));
  return applicationInfoResults.getEntity();
}
