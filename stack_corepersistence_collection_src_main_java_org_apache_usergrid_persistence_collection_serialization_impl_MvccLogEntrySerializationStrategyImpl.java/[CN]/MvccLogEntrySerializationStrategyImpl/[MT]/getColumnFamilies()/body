{
  MultiTennantColumnFamilyDefinition cf=new MultiTennantColumnFamilyDefinition(CF_ENTITY_LOG,BytesType.class.getSimpleName(),ReversedType.class.getSimpleName() + "(" + UUIDType.class.getSimpleName()+ ")",IntegerType.class.getSimpleName(),MultiTennantColumnFamilyDefinition.CacheOption.KEYS);
