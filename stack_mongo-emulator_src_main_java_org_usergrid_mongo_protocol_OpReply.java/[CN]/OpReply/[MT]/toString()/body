{
  String docs_str=null;
  try {
    docs_str=documents.toString();
  }
 catch (  Exception e) {
    docs_str="error(" + e.getMessage() + ")";
  }
  return "OpReply [responseFlags=" + responseFlags + ", cursorID="+ cursorID+ ", startingFrom="+ startingFrom+ ", numberReturned="+ numberReturned+ ", documents="+ docs_str+ "]";
}
