{
  UserRepo.INSTANCE.load(resource(),access_token);
  refreshIndex("test-organization","test-app");
  String ql="picture = 'foo' ";
  resource().path("/test-organization/test-app/users").queryParam("ql",ql).queryParam("access_token",access_token).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).get(String.class);
}
