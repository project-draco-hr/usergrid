{
  if ((byteBuffer == null) || !byteBuffer.hasRemaining()) {
    return null;
  }
  if (clazz == null) {
    clazz=Object.class;
  }
  ObjectMapper mapper=new ObjectMapper(smile);
  Object obj=null;
  try {
    obj=mapper.readValue(byteBuffer.array(),byteBuffer.arrayOffset() + byteBuffer.position(),byteBuffer.remaining(),clazz);
  }
 catch (  Exception e) {
    logger.error("Error parsing SMILE bytes",e);
  }
  return obj;
}
