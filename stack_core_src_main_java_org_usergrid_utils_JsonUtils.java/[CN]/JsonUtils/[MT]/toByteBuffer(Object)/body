{
  if (obj == null) {
    return null;
  }
  ObjectMapper mapper=new ObjectMapper(smile);
  byte[] bytes=null;
  try {
    bytes=mapper.writeValueAsBytes(obj);
  }
 catch (  Exception e) {
    logger.error("Error getting SMILE bytes",e);
  }
  if (bytes != null) {
    return ByteBuffer.wrap(bytes);
  }
  return null;
}
