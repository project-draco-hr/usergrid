{
  if ((byteBuffer == null) || !byteBuffer.hasRemaining()) {
    return null;
  }
  ObjectMapper mapper=new ObjectMapper(smile);
  JsonNode obj=null;
  try {
    obj=mapper.readValue(byteBuffer.array(),byteBuffer.arrayOffset() + byteBuffer.position(),byteBuffer.remaining(),JsonNode.class);
  }
 catch (  Exception e) {
    logger.error("Error parsing SMILE bytes",e);
  }
  return obj;
}
