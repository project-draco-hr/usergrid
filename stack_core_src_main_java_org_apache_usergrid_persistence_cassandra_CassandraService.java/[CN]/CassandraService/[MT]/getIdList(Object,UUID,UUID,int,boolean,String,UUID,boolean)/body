{
  if (count <= 0) {
    count=DEFAULT_COUNT;
  }
  if (NULL_ID.equals(start)) {
    start=null;
  }
  final boolean skipFirst=start != null && !keepFirst;
  IndexScanner scanner=new IndexBucketScanner<UUID>(this,ENTITY_ID_SETS,UUIDStartToBytes.INSTANCE,applicationId,key,bucket,start,finish,reversed,count,skipFirst);
  return scanner;
}
