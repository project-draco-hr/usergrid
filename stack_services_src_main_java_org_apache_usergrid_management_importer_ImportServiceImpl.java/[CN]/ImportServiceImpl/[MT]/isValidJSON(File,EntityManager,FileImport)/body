{
  boolean valid=false;
  try {
    final JsonParser jp=jsonFactory.createJsonParser(collectionFile);
    while (jp.nextToken() != null) {
    }
    valid=true;
  }
 catch (  JsonParseException e) {
    e.printStackTrace();
    fileImport.setErrorMessage(e.getMessage());
    rootEm.update(fileImport);
  }
catch (  IOException e) {
    fileImport.setErrorMessage(e.getMessage());
    rootEm.update(fileImport);
  }
  return valid;
}
