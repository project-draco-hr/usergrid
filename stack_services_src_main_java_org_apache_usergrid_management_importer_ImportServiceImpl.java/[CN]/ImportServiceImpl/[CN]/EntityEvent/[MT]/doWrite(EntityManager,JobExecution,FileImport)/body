{
  EntityManager rootEm=emf.getEntityManager(CpNamingUtils.MANAGEMENT_APPLICATION_ID);
  try {
    logger.debug("Writing imported entity {}:{} into app {}",new Object[]{entityType,entityUuid,em.getApplication().getUuid()});
    em.create(entityUuid,entityType,properties);
  }
 catch (  Exception e) {
    logger.error("Error writing entity",e);
    fileImport.setErrorMessage(e.getMessage());
    try {
      rootEm.update(fileImport);
    }
 catch (    Exception ex) {
      logger.error("Error updating file import report with error message: " + fileImport.getErrorMessage(),ex);
    }
  }
}
