{
  rmdir(TMP);
  copy("/log4j.properties",TMP);
  copy(yamlFile,TMP);
  System.setProperty("cassandra.config","file:" + TMP + yamlFile);
  System.setProperty("log4j.configuration","file:" + TMP + "/log4j.properties");
  System.setProperty("cassandra-foreground","true");
  cleanupAndLeaveDirs();
  loadSchemaFromYaml();
}
