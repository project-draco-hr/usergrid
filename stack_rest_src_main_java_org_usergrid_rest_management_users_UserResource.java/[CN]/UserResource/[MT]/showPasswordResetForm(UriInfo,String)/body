{
  try {
    this.token=token;
    if (management.checkPasswordResetTokenForAdminUser(user.getUuid(),token)) {
      return handleViewable("resetpw_set_form",this);
    }
 else {
      return handleViewable("resetpw_email_form",this);
    }
  }
 catch (  Exception e) {
    return handleViewable("error",e);
  }
}
