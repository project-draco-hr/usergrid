{
  ApiResponse response=new ApiResponse(ui);
  response.setAction("get admin user");
  String token=management.getAccessTokenForAdminUser(SubjectUtils.getUser().getUuid());
  Map<String,Object> userOrganizationData=management.getAdminUserOrganizationData(user.getUuid());
  userOrganizationData.put("token",token);
  response.setData(userOrganizationData);
  response.setSuccess();
  return response;
}
