{
  logger.info("Filtering " + request.getRequestUri().toString());
  if (request.getMethod().equalsIgnoreCase("OPTIONS")) {
    logger.info("Skipping option request");
    return request;
  }
  MultivaluedMap<java.lang.String,java.lang.String> params=uriInfo.getPathParameters();
  logger.info("Params: " + params.keySet());
  authorize(request);
  return request;
}
