{
  if (q != null) {
    limit=q.limit;
    limitSet=q.limitSet;
    startTime=q.startTime;
    finishTime=q.finishTime;
    resolution=q.resolution;
    pad=q.pad;
    categories=q.categories != null ? new ArrayList<String>(q.categories) : null;
    counterFilters=q.counterFilters != null ? new ArrayList<CounterFilterPredicate>(q.counterFilters) : null;
  }
}
