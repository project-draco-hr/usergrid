{
  init();
  this.connections=connections;
  this.forwardConnections=forwardConnections;
  level=Level.REFS;
  for (  ConnectionRef connection : connections) {
    if (forwardConnections) {
      this.setMetadata(connection.getConnectedEntity().getUuid(),"connection",connection.getConnectionType());
    }
 else {
      this.setMetadata(connection.getConnectingEntity().getUuid(),"connection",connection.getConnectionType());
    }
  }
}
