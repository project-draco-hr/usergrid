{
  ArrayList<UsergridUser> users=null;
  if (entities != null && !entities.isEmpty()) {
    for (    UsergridEntity entity : entities) {
      if (entity instanceof UsergridUser) {
        if (users == null) {
          users=new ArrayList<>();
        }
        users.add((UsergridUser)entity);
      }
    }
  }
  return users;
}
