{
  Properties props=PropertyUtils.loadFromClassPath(CASS_PROPS);
  DynamicPropertyNames.bindProperties(binder(),props);
  DynamicPropertyNames.bindProperties(binder(),PropertyUtils.loadSystemProperties(AstynaxKeyspaceProvider.getRuntimeOptions()));
  install(new SerializationModule());
  install(new ServiceModule());
  install(new FactoryModuleBuilder().implement(EntityCollectionManager.class,EntityCollectionManagerImpl.class).build(EntityCollectionManagerFactory.class));
}
