{
  JsonNode node=resource().path("/management/token").queryParam("grant_type","password").queryParam("username",user).queryParam("password",password).accept(MediaType.APPLICATION_JSON).get(JsonNode.class);
  String mgmToken=node.get("access_token").textValue();
  logger.info("got mgmt token: {}",mgmToken);
  return mgmToken;
}
