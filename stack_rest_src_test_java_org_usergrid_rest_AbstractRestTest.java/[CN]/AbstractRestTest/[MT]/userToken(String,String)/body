  setUserPassword("ed@anuff.com","sesame");
  JsonNode node=resource().path("/test-organization/test-app/token").queryParam("grant_type","password").queryParam("username",name).queryParam("password",password).accept(MediaType.APPLICATION_JSON).get(JsonNode.class);
