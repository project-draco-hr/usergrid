{
  return Observable.create(new ObservableIterator<MarkedEdge>(){
    @Override protected Iterator<MarkedEdge> getIterator(){
      final SimpleSearchByEdge search=new SimpleSearchByEdge(edge.getSourceNode(),edge.getType(),edge.getTargetNode(),edge.getVersion(),null);
      return edgeSerialization.getEdgeFromSource(scope,search);
    }
  }
).subscribeOn(scheduler);
}
