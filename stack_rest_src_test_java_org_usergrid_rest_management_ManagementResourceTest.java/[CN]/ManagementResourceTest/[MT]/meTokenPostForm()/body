{
  JsonNode node=resource().path("/management/me").queryParam("grant_type","password").queryParam("username","test@usergrid.com").queryParam("password","test").accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_FORM_URLENCODED_TYPE).post(JsonNode.class);
  logNode(node);
  String token=node.get("access_token").textValue();
  assertNotNull(token);
  node=resource().path("/management/me").queryParam("access_token",token).accept(MediaType.APPLICATION_JSON).get(JsonNode.class);
  logNode(node);
}
