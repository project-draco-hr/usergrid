{
  Map<String,String> payload=hashMap("grant_type","password").map("username","test@usergrid.com").map("password","test");
  JsonNode node=resource().path("/management/me").accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(JsonNode.class,payload);
  logNode(node,LOG);
  String token=node.get("access_token").getTextValue();
  assertNotNull(token);
  node=resource().path("/management/me").queryParam("access_token",token).accept(MediaType.APPLICATION_JSON).get(JsonNode.class);
  logNode(node,LOG);
}
