{
  try {
    jg.writeStartObject();
    jg.writeString(MetricSort.APP_REQ_COUNT.toString());
    jg.writeStartArray();
    for (    MetricLine ml : sortDelegator(scoreMaps,MetricSort.APP_REQ_COUNT)) {
      jg.writeObject(ml);
    }
    jg.writeEndArray();
    jg.writeEndObject();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
