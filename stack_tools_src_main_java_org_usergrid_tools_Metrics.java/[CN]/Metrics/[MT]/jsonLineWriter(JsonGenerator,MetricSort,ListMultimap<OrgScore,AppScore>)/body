{
  try {
    jg.writeStartObject();
    jg.writeFieldName(metricSort.name());
    jg.writeStartArray();
    for (    MetricLine ml : sortDelegator(scoreMaps,metricSort)) {
      jg.writeObject(ml);
    }
    jg.writeEndArray();
    jg.writeEndObject();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
}
