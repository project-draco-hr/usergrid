{
  log.debug("submit triggered...");
  Future f=batchSubmitter.submit(batch);
  if (blockingSubmit) {
    try {
      f.get();
    }
 catch (    InterruptedException e) {
      e.printStackTrace();
    }
catch (    ExecutionException e) {
      e.printStackTrace();
    }
  }
  batchSubmissionCount.incrementAndGet();
}
