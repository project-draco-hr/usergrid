{
  Settings settings=ImmutableSettings.settingsBuilder().build();
  client=new TransportClient(settings).addTransportAddress(new InetSocketTransportAddress(elasticFig.getTransportHost(),elasticFig.getTransportPort()));
  port=elasticFig.getTransportPort();
  host=elasticFig.getTransportHost();
  clusterName=elasticFig.getClusterName();
}
