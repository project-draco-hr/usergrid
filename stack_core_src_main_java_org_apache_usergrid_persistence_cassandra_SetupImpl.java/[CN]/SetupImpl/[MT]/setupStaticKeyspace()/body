{
  logger.info("Creating static application keyspace " + getApplicationKeyspace());
  cass.createColumnFamily(getApplicationKeyspace(),createColumnFamilyDefinition(getApplicationKeyspace(),APPLICATIONS_CF,ComparatorType.BYTESTYPE));
  cass.createColumnFamilies(getApplicationKeyspace(),getCfDefs(ApplicationCF.class,getApplicationKeyspace()));
  cass.createColumnFamilies(getApplicationKeyspace(),getCfDefs(QueuesCF.class,getApplicationKeyspace()));
}
