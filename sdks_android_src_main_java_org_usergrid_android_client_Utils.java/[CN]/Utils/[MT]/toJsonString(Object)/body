{
  try {
    return mapper.writeValueAsString(obj);
  }
 catch (  JsonGenerationException e) {
    Log.e(TAG,"ERROR: " + e.getLocalizedMessage());
  }
catch (  JsonMappingException e) {
    Log.e(TAG,"ERROR: " + e.getLocalizedMessage());
  }
catch (  IOException e) {
    Log.e(TAG,"ERROR: " + e.getLocalizedMessage());
  }
  return "{}";
}
