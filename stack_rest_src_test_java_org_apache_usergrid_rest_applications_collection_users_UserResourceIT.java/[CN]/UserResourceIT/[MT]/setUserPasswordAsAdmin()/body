{
  String newPassword="foo";
  Map<String,String> data=new HashMap<String,String>();
  data.put("newpassword",newPassword);
  JsonNode node=mapper.readTree(resource().path("/test-organization/test-app/users/edanuff/password").queryParam("access_token",adminAccessToken).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(String.class,data));
  assertNull(getError(node));
  ApiResponse response=client.authorizeAppUser("ed@anuff.com",newPassword);
  assertNull(response.getError());
}
