{
  Map<String,String> payload=new HashMap<String,String>();
  JsonNode node=mapper.readTree(resource().path("/test-organization/test-app/cities").queryParam("access_token",access_token).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(String.class,payload));
  assertNull(getEntity(node,0));
  assertNull(node.get("count"));
}
