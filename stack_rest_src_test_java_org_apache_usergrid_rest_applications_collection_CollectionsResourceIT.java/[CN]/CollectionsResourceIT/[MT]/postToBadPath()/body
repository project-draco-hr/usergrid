{
  Map<String,String> payload=hashMap("name","Austin").map("state","TX");
  JsonNode node=null;
  try {
    node=mapper.readTree(resource().path("/test-organization/test-organization/test-app/cities").queryParam("access_token",access_token).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(String.class,payload));
  }
 catch (  UniformInterfaceException e) {
    assertEquals("Should receive a 400 Not Found",400,e.getResponse().getStatus());
  }
}
