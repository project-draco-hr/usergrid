{
  JsonNode groupJson=resource().path(context.getOrgName() + "/" + context.getAppName()+ "/groups/"+ groupId).queryParam("access_token",context.getActiveUser().getToken()).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).get(JsonNode.class);
  Assert.assertEquals(title,groupJson.get("entities").get(0).get("title").getTextValue());
}
