{
  try {
    incrementAggregateCounters(null,null,null,new String(APPLICATION_COLLECTION + collection_name),ONE_COUNT,cassandraTimestamp);
  }
 catch (  Exception e) {
    logger.error("Unable to increment counter application.collection." + collection_name,e);
  }
