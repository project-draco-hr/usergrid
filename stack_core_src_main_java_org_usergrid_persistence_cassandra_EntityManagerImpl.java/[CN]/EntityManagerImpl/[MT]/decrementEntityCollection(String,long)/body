{
  try {
    incrementAggregateCounters(null,null,null,APPLICATION_COLLECTION + collection_name,-ONE_COUNT,cassandraTimestamp);
  }
 catch (  Exception e) {
    logger.error("Unable to decrement counter application.collection." + collection_name,e);
  }
