{
  if ((userId == null) || (password == null)) {
    return false;
  }
  EntityManager em=emf.getEntityManager(MANAGEMENT_APPLICATION_ID);
  Entity user=em.get(userId);
  if (checkPassword(password,(CredentialsInfo)em.getDictionaryElementValue(user,DICTIONARY_CREDENTIALS,"password"))) {
    return true;
  }
  return false;
}
