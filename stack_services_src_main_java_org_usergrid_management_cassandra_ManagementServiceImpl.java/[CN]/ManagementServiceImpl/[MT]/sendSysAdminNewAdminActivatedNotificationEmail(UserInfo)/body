{
  if (notifySysAdminOfNewAdminUsers()) {
    sendHtmlMail(properties,getPropertyValue(PROPERTIES_SYSADMIN_EMAIL),getPropertyValue(PROPERTIES_MAILER_EMAIL),"Admin User Account Activated " + user.getEmail(),appendEmailFooter(emailMsg(hashMap("user_email",user.getEmail()),PROPERTIES_EMAIL_SYSADMIN_ADMIN_ACTIVATED)));
  }
}
