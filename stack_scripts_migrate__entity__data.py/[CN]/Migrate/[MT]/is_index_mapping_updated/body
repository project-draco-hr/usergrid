def is_index_mapping_updated(self):
    status = self.check_data_migration_status()
    if (status is not None):
        migration_system_version = status['data'][PLUGIN_INDEX_MAPPING]
        if (migration_system_version == TARGET_INDEX_MAPPING_VERSION):
            self.logger.info('Index Mapping CURRENT, %s=[%s]', PLUGIN_MIGRATION_SYSTEM, migration_system_version)
            return True
        else:
            self.logger.info('Index Mapping OLD, %s=[%s]', PLUGIN_MIGRATION_SYSTEM, migration_system_version)
    return False
