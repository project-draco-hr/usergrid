{
  StringBuilder str=new StringBuilder();
  str.append(organizationName);
  str.append("/");
  str.append(name);
  str.append(".");
  if (CollectionName != null) {
    str.append(CollectionName);
    str.append(".");
  }
  str.append("*");
  str.append(".json");
  String inputFileName=str.toString();
  return inputFileName;
}
