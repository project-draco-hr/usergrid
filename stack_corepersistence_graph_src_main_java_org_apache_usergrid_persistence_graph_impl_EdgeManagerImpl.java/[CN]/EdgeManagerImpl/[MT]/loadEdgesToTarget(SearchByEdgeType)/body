{
  return Observable.create(new ObservableIterator<Edge>(){
    @Override protected Iterator<Edge> getIterator(){
      return edgeSerialization.getEdgesToTarget(scope,search);
    }
  }
).filter(new MaxFilter(search.getMaxVersion())).distinctUntilChanged(new Func1<Edge,Id>(){
    @Override public Id call(    final Edge edge){
      return edge.getSourceNode();
    }
  }
);
}
