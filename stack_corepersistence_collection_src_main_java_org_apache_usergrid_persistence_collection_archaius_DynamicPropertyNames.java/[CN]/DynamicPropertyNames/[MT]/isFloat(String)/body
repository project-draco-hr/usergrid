{
  if (!(value.endsWith("F") || value.endsWith("f"))) {
    return false;
  }
  try {
    if (value.endsWith("F") || value.endsWith("f")) {
      Float.parseFloat(value.substring(0,value.length() - 1));
    }
 else {
      Float.parseFloat(value);
    }
    return true;
  }
 catch (  NumberFormatException e) {
    return false;
  }
}
