{
  if (value.contains(".") || value.contains("E") || value.contains("e")|| value.contains("/")|| value.endsWith("F")|| value.endsWith("f")|| value.endsWith("L")|| value.endsWith("l")) {
    return false;
  }
  try {
    Integer.valueOf(value);
    return true;
  }
 catch (  NumberFormatException e) {
    return false;
  }
}
