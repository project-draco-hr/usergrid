{
  final String threadName=Thread.currentThread().getName();
  logger.info("Function executing on thread {}",threadName);
  set.add(threadName);
  try {
    latch.countDown();
    latch.await(30,TimeUnit.SECONDS);
  }
 catch (  InterruptedException e) {
    logger.error("Runner interrupted",e);
  }
  return s;
}
