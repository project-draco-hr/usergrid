{
  try {
    int _type=LT;
    int _channel=DEFAULT_TOKEN_CHANNEL;
    int alt1=2;
switch (input.LA(1)) {
case '<':
{
        alt1=1;
      }
    break;
case 'l':
{
    alt1=2;
  }
break;
default :
NoViableAltException nvae=new NoViableAltException("",1,0,input);
throw nvae;
}
switch (alt1) {
case 1:
{
match('<');
}
break;
case 2:
{
match("lt");
}
break;
}
state.type=_type;
state.channel=_channel;
}
  finally {
}
}
