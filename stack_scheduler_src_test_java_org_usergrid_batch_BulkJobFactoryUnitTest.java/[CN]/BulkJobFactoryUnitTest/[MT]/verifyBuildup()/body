{
  JobDescriptor jobDescriptor=JobDescriptor.instance("my-job-descriptor",BulkTestUtils.getBulkJobFactory(),jobId);
  BulkJobsBuilder bulkJobsBuilder=BulkJobsBuilder.newBuilder(jobDescriptor).maxExecTime(1234L).limit(5);
  List<BulkJob> bulkJobs=bulkJobsBuilder.build();
  assertNotNull(bulkJobs);
  assertEquals(1,bulkJobs.size());
}
