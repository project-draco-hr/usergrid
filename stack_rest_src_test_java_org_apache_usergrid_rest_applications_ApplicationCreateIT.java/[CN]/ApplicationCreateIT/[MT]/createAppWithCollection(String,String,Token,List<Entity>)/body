{
  ApiResponse appCreateResponse=clientSetup.getRestClient().management().orgs().organization(orgName).app().post(new Application(appName));
  UUID appId=appCreateResponse.getEntities().get(0).getUuid();
  refreshIndex();
  for (int i=0; i < 5; i++) {
    final String entityName="entity" + i;
    Entity entity=new Entity();
    entity.setProperties(new HashMap<String,Object>(){
{
        put("name",entityName);
      }
    }
);
    ApiResponse createResponse=clientSetup.getRestClient().org(orgName).app(appName).collection("things").getResource().queryParam("access_token",orgAdminToken.getAccessToken()).type(MediaType.APPLICATION_JSON).post(ApiResponse.class,entity);
    entities.add(createResponse.getEntities().get(0));
  }
  return appId;
}
