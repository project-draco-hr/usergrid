{
  final APNsNotification apnsNotification=APNsNotification.create(providerId,payload.toString(),notification,tracker);
  if (pool == null) {
    apnsNotification.messageSent();
  }
 else {
    pool.submit(new Runnable(){
      @Override public void run(){
        try {
          Thread.sleep(new Random().nextInt(100));
          apnsNotification.messageSent();
        }
 catch (        Exception e) {
          e.printStackTrace();
        }
      }
    }
);
  }
}
