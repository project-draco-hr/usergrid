{
  if (this == obj) {
    return true;
  }
  if (obj == null) {
    return false;
  }
  if (getClass() != obj.getClass()) {
    return false;
  }
  EntityLocationRef other=(EntityLocationRef)obj;
  if (type == null) {
    if (other.type != null) {
      return false;
    }
  }
 else   if (!type.equals(other.type)) {
    return false;
  }
  if (uuid == null) {
    if (other.uuid != null) {
      return false;
    }
  }
 else   if (!uuid.equals(other.uuid)) {
    return false;
  }
  return true;
}
