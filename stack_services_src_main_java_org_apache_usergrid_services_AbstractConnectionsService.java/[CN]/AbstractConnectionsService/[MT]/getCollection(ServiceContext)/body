{
  checkPermissionsForCollection(context);
  Results r=null;
  if (connecting()) {
    r=em.getConnectingEntities(new SimpleEntityRef(context.getOwner().getType(),context.getOwner().getUuid()),context.getCollectionName(),null,Level.ALL_PROPERTIES);
  }
 else {
    r=em.getConnectedEntities(new SimpleEntityRef(context.getOwner().getType(),context.getOwner().getUuid()),context.getCollectionName(),null,Level.ALL_PROPERTIES);
  }
  importEntities(context,r);
  return new ServiceResults(this,context,Type.CONNECTION,r,null,null);
}
