{
  Options options=super.createOptions();
  Option appNameOption=OptionBuilder.hasArg().withType("").withDescription("Application Name -" + APPLICATION_NAME).create(APPLICATION_NAME);
  options.addOption(appNameOption);
  Option readThreadsOption=OptionBuilder.hasArg().withType(0).withDescription("Read Threads -" + READ_THREAD_COUNT).create(READ_THREAD_COUNT);
  options.addOption(readThreadsOption);
  Option writeThreadsOption=OptionBuilder.hasArg().withType(0).withDescription("Write Threads -" + WRITE_THREAD_COUNT).create(WRITE_THREAD_COUNT);
  options.addOption(writeThreadsOption);
  return options;
}
