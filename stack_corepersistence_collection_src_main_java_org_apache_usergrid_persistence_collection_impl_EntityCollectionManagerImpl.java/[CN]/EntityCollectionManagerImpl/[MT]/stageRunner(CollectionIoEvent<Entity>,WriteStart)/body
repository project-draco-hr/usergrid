{
  return Observable.just(writeData).map(writeState).doOnNext(new Action1<CollectionIoEvent<MvccEntity>>(){
    @Override public void call(    final CollectionIoEvent<MvccEntity> mvccEntityCollectionIoEvent){
      Observable<CollectionIoEvent<MvccEntity>> unique=Observable.just(mvccEntityCollectionIoEvent).subscribeOn(Schedulers.io()).doOnNext(writeVerifyUnique);
      Observable<CollectionIoEvent<MvccEntity>> optimistic=Observable.just(mvccEntityCollectionIoEvent).subscribeOn(Schedulers.io()).doOnNext(writeOptimisticVerify);
      Observable.merge(unique,optimistic).toBlocking().last();
    }
  }
);
}
