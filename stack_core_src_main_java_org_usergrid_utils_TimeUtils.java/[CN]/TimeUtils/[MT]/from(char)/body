{
switch (c) {
case 's':
    return SEC_TOKEN;
case 'm':
  return MIN_TOKEN;
case 'h':
return HOUR_TOKEN;
case 'd':
return DAY_TOKEN;
case 'S':
return MILSEC_TOKEN;
}
if (CharMatcher.DIGIT.matches(c)) {
return MILSEC_TOKEN;
}
throw new IllegalArgumentException("Duration token was not on of [S,s,m,h,d] but was " + c);
}
