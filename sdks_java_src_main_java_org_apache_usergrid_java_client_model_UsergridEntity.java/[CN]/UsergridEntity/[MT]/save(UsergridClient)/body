{
  UsergridResponse response;
  if (this.getUuid() != null) {
    response=client.PUT(this);
  }
 else {
    response=client.POST(this);
  }
  if (response.ok()) {
    UsergridEntity responseEntity=response.first();
    if (responseEntity != null) {
      this.copyAllProperties(responseEntity);
    }
  }
  return response;
}
