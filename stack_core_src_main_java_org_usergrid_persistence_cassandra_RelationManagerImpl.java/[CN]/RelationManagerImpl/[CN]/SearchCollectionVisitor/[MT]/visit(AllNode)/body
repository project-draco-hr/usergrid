{
  String collectionName=collection.getName();
  IndexScanner results=cass.getIdList(cass.getApplicationKeyspace(applicationId),key(headEntity.getUuid(),DICTIONARY_COLLECTIONS,collectionName),query.getStartResult(),null,query.getLimit() + 1,query.isReversed(),indexBucketLocator,applicationId,collectionName);
  this.results.push(new SliceIterator<UUID>(results,node.getSlice(),UUID_PARSER));
}
