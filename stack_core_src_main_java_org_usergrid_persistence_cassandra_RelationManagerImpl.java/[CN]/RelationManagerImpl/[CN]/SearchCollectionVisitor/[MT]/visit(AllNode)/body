{
  String collectionName=collection.getName();
  QuerySlice slice=node.getSlice();
  queryProcessor.applyCursorAndSort(slice);
  UUID startId=null;
  if (slice.hasCursor()) {
    startId=UUIDSerializer.get().fromByteBuffer(slice.getCursor());
  }
  IndexScanner results=cass.getIdList(cass.getApplicationKeyspace(applicationId),key(headEntity.getUuid(),DICTIONARY_COLLECTIONS,collectionName),startId,null,query.getLimit(),query.isReversed(),indexBucketLocator,applicationId,collectionName);
  this.results.push(new SliceIterator<UUID>(results,node.getSlice(),UUID_PARSER));
}
