{
  List<EntityLocationRef> locations=em.getGeoIndexManager().proximitySearchConnections(connection.getIndexId(),node.getPropertyName(),new Point(node.getLattitude(),node.getLongitude()),node.getDistance(),null,PAGE_SIZE,false,query.getResultsLevel());
  GeoIterator itr=new GeoIterator(locations,PAGE_SIZE);
  results.push(itr);
}
