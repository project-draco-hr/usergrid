{
  int limit=queryProcessor.getFirstNode() == node ? query.getLimit() : GEO_MAX;
  Results r=em.getGeoIndexManager().proximitySearchConnections(connection.getIndexId(),node.getPropertyName(),new Point(node.getLattitude(),node.getLongitude()),node.getDistance(),null,limit,false,query.getResultsLevel());
  results.push(r);
}
