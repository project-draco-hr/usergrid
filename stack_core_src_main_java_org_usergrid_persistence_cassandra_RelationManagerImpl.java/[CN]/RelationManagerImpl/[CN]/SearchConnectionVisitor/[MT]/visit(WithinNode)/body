{
  QuerySlice slice=node.getSlice();
  queryProcessor.applyCursorAndSort(slice);
  GeoIterator itr=new GeoIterator(new GeoIterator.ConnectionGeoSearch(em.getGeoIndexManager(),connection.getIndexId(),new Point(node.getLattitude(),node.getLongitude()),node.getPropertyName(),node.getDistance()),query.getLimit(),slice);
  results.push(itr);
}
