{
  Object start=getStart(slice);
  Object finish=getFinish(slice);
  if (slice.isReversed() && (start != null) && (finish != null)) {
    Object temp=start;
    start=finish;
    finish=temp;
  }
  return cass.getColumns(cass.getApplicationKeyspace(applicationId),ENTITY_INDEX,key(indexKey,slice.getPropertyName()),start,finish,count,slice.isReversed());
}
