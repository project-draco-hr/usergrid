{
  log.info("Entering before");
synchronized (mutex) {
    clientCount.incrementAndGet();
    if (started) {
      log.info("NOT starting Tomcat because it is already started #2");
      return;
    }
    started=true;
    if (forkTomcat) {
      process=startTomcatProcess();
    }
 else {
      startTomcatEmbedded();
    }
  }
  log.info("Leaving before");
}
