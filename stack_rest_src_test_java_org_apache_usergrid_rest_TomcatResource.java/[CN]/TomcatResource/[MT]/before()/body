{
  if (started) {
    return;
  }
synchronized (mutex) {
    if (started) {
      return;
    }
    port=AvailablePortFinder.getNextAvailable(9998 + RandomUtils.nextInt(10));
    String propDirPath=createPropDir();
    createPropertyFiles(propDirPath);
    process=startTomcatProcess(propDirPath);
    waitForTomcat();
    started=true;
  }
}
