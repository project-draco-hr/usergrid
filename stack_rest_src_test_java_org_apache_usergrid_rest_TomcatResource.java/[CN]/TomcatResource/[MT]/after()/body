{
  log.info("Entering after");
synchronized (mutex) {
    if (clientCount.decrementAndGet() == 0) {
      log.info("Destroying Tomcat running on port " + port);
      process.destroy();
      started=false;
    }
 else {
      log.info("NOT stopping Tomcat because it is still in use");
    }
  }
  log.info("Leaving after");
}
