{
  for (int i=0; i < Math.min(cell1.length(),cell2.length()); i++) {
    int l1[]=subdivXY(cell1.charAt(i));
    int x1=l1[0];
    int y1=l1[1];
    int l2[]=subdivXY(cell2.charAt(i));
    int x2=l2[0];
    int y2=l2[1];
    if (!columnTest && y1 != y2) {
      return false;
    }
    if (columnTest && x1 != x2) {
      return false;
    }
  }
  return true;
}
