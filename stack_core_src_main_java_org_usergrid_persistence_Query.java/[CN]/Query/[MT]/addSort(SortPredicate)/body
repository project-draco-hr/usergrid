{
  if (sort == null) {
    return this;
  }
  for (  SortPredicate s : sortPredicates) {
    if (s.getPropertyName().equals(sort.getPropertyName())) {
      logger.error("Attempted to set sort order for " + s.getPropertyName() + " more than once, discarding...");
      return this;
    }
  }
  sortPredicates.add(sort);
  return this;
}
