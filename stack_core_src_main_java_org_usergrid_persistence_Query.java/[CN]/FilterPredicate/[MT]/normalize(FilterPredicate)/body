{
  if (p == null) {
    return null;
  }
  if (p.operator == FilterOperator.CONTAINS) {
    String propertyName=p.propertyName;
    if (StringUtils.isNotEmpty(propertyName)) {
      propertyName+=".keywords";
    }
 else {
      propertyName="keywords";
    }
    return new FilterPredicate(propertyName,FilterOperator.EQUAL,p.value);
  }
  return p;
}
