{
  entitiesMap=null;
  if ((this.entity != null) && (this.entity.getId().equals(entity.getId()))) {
    this.entity=entity;
  }
  if (entities != null) {
    ListIterator<Entity> i=entities.listIterator();
    while (i.hasNext()) {
      Entity e=i.next();
      if (e.getId().equals(entity.getId())) {
        i.set(entity);
      }
    }
  }
}
