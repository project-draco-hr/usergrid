{
  if (refsMap != null) {
    return refsMap;
  }
  getEntitiesMap();
  if (entitiesMap != null) {
    refsMap=cast(entitiesMap);
    return refsMap;
  }
  getRefs();
  if (refs != null) {
    refsMap=new LinkedHashMap<Id,EntityRef>();
    for (    EntityRef ref : refs) {
      refsMap.put(ref.getId(),ref);
    }
  }
  return refsMap;
}
