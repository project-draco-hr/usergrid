{
  final String value=indexProcessorFig.getQueueImplementation();
  final Implementations impl=Implementations.valueOf(value);
switch (impl) {
case LOCAL:
    return new InMemoryAsyncIndexService(indexService,rxTaskScheduler,entityCollectionManagerFactory,indexProcessorFig.resolveSynchronously());
case SQS:
  return new SQSAsyncIndexService(queueManagerFactory,indexProcessorFig,metricsFactory,indexService,entityCollectionManagerFactory,rxTaskScheduler);
default :
throw new IllegalArgumentException("Configuration value of " + getErrorValues() + " are allowed");
}
}
