{
  final EdgeShardStrategy sizeBasedStrategy=new SizebasedEdgeShardStrategy(cache,shardApproximation);
  final ShardedEdgeSerialization serialization=new ShardedEdgeSerializationImpl(keyspace,cassandraConfig,graphFig,sizeBasedStrategy,timeService);
  final EdgeSerializationImpl edgeSerialization=new EdgeSerializationImpl(keyspace,cassandraConfig,graphFig,sizeBasedStrategy,edgeColumnFamilies,serialization);
  return edgeSerialization;
}
