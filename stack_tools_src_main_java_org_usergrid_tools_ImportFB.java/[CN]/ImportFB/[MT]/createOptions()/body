{
  Option input=OptionBuilder.withArgName("file").hasArg().isRequired().withDescription("JSON file of array of Facebook users").create("i");
  Option output=OptionBuilder.withArgName("file").hasArg().isRequired().withDescription("JSON file with array of Usergrid users").create("o");
  Options options=new Options();
  options.addOption(input);
  options.addOption(output);
  return options;
}
