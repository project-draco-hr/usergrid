{
  Map<String,Object> params=new HashMap<String,Object>();
  params.put("token",token);
  ApiResponse response=apiRequest(HTTP_METHOD_PUT,params,null,organizationId,applicationId,"users",username,"revoketoken?");
  if (response == null) {
    return response;
  }
 else {
    logInfo("logoutAppWithTokenUser(): Response: " + response);
    if (token.equals(getAccessToken())) {
      setAccessToken(null);
    }
  }
  return response;
}
