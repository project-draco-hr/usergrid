{
  ByteBuffer rowKey=principalKey(principal);
  List<HColumn<ByteBuffer,ByteBuffer>> cols=cassandra.getColumns(cassandra.getSystemKeyspace(),PRINCIPAL_TOKEN_CF,rowKey,null,null,Integer.MAX_VALUE,false);
  List<UUID> results=new ArrayList<UUID>(cols.size());
  for (  HColumn<ByteBuffer,ByteBuffer> col : cols) {
    results.add(uuid(col.getName()));
  }
  return results;
}
