{
  String responseString=getResource(true).type(MediaType.APPLICATION_JSON_TYPE).accept(MediaType.APPLICATION_JSON).get(String.class);
  logger.debug("Response from post: " + responseString);
  org.apache.usergrid.rest.test.resource.model.ApiResponse response;
