{
  String mgmtToken=context.getActiveUser().getToken();
  ClientResponse.Status status=null;
  JsonNode node=null;
  try {
    node=mapper.readTree(resource().path(context.getOrgName()).queryParam("access_token",mgmtToken).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).delete(String.class));
  }
 catch (  UniformInterfaceException uie) {
    status=uie.getResponse().getClientResponseStatus();
  }
  assertEquals(ClientResponse.Status.NOT_IMPLEMENTED,status);
}
