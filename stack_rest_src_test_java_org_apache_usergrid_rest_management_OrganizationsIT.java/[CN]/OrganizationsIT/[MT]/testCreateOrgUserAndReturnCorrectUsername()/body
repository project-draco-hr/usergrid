{
  String mgmtToken=context.getActiveUser().getToken();
  Map<String,String> payload=hashMap("username","test-user-2").map("name","Test User 2").map("email","test-user-2@mockserver.com").map("password","password");
  JsonNode node=mapper.readTree(resource().path("/management/organizations/" + context.getOrgName() + "/users").queryParam("access_token",mgmtToken).accept(MediaType.APPLICATION_JSON).type(MediaType.APPLICATION_JSON_TYPE).post(String.class,payload));
  logNode(node);
  assertNotNull(node);
  String username=node.get("data").get("user").get("username").asText();
  String name=node.get("data").get("user").get("name").asText();
  String email=node.get("data").get("user").get("email").asText();
  assertNotNull(username);
  assertNotNull(name);
  assertNotNull(email);
  assertEquals("test-user-2",username);
  assertEquals("Test User 2",name);
  assertEquals("test-user-2@mockserver.com",email);
}
