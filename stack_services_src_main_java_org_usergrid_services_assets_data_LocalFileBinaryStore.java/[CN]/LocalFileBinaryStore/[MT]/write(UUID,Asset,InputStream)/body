{
  File file=path(appId,asset);
  try {
    FileUtils.copyInputStreamToFile(inputStream,file);
    long size=FileUtils.sizeOf(file);
    asset.setProperty("content-length",size);
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  if (file.exists()) {
    AssetMimeHandler.get().getMimeType(asset,file);
  }
}
